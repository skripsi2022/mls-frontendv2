{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const kelas = reactive({\n      nama_kelas: '',\n      jurusan_id: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/kelas/${route.params.id}`).then(result => {\n        kelas.nama_kelas = result.data.data.nama_kelas;\n        kelas.jurusan_id = result.data.data.jurusan_id;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n\n    function update() {\n      axios.put(`http://localhost:8000/api/kelas/${route.params.id}`, kelas).then(() => {\n        router.push({\n          name: 'kelas.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      kelas,\n      Validation,\n      router,\n      update\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiEA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIP,QAAQ,CAAC;AACnBQ,gBAAU,EAAE,EADO;AAEnBC,gBAAU,EAAE;AAFO,KAAD,CAAtB;AAKA,UAAMC,UAAS,GAAIT,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMU,MAAK,GAAIP,SAAS,EAAxB;AACA,UAAMQ,KAAI,GAAIT,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACQ,GAAN,CAAW,mCAAkCD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA7D,EACCC,IADD,CACOC,MAAD,IAAY;AACdV,aAAK,CAACC,UAAN,GAAmBS,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBV,UAApC;AACAD,aAAK,CAACE,UAAN,GAAmBQ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBT,UAApC;AACH,OAJD,EAIGU,KAJH,CAIUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OAND;AAOH,KARQ,CAAT;;AAUA,aAASM,MAAT,GAAkB;AACdnB,WAAK,CAACoB,GAAN,CACK,mCAAkCb,KAAK,CAACE,MAAN,CAAaC,EAAG,EADvD,EAEIR,KAFJ,EAIES,IAJF,CAIO,MAAM;AACTL,cAAM,CAACe,IAAP,CAAY;AACRC,cAAG,EAAI;AADC,SAAZ;AAGH,OARD,EAQGR,KARH,CAQUC,GAAD,IAAS;AACdV,kBAAU,CAACkB,KAAX,GAAmBR,GAAG,CAACG,QAAJ,CAAaL,IAAhC;AACH,OAVD;AAWJ;;AAEA,WAAO;AACHX,WADG;AAEHG,gBAFG;AAGHC,YAHG;AAIHa;AAJG,KAAP;AAMJ;;AA3CW,CAAf","names":["reactive","ref","onMounted","useRoute","useRouter","axios","setup","kelas","nama_kelas","jurusan_id","Validation","router","route","get","params","id","then","result","data","catch","err","console","log","response","update","put","push","name","value"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Kelas\\DetailView.vue"],"sourcesContent":["<template>\r\n     <div class=\"content container\">\r\n                <div class=\"main\">\r\n                    <div class=\"row\">\r\n                        <div class=\"col-lg-8\">\r\n                            <div class=\"page-header\">\r\n                                <h4 class=\"page-title\">Jurusan</h4>\r\n                                <div class=\"breadcrumb\">\r\n                                <span class=\"me-1 text-gray\"><i class=\"feather icon-home\"></i></span>\r\n                                <div class=\"breadcrumb-item\">Home</div>\r\n                                <div class=\"breadcrumb-item\">Kelas</div>\r\n                                <div class=\"breadcrumb-item\">Detail</div>\r\n                                </div>                         \r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-lg-4\">\r\n                             <div class=\"d-grid d-md-flex justify-content-md-end\">\r\n                                <router-link :to=\"{name: 'jurusan.index'}\" class=\"btn btn-warning me-md-end\" type=\"button\"><i class=\"icon-arrow-left-circle feather\"></i>Kembali</router-link>\r\n                             </div>\r\n                        </div>\r\n                    </div>\r\n                    <br>\r\n                    <div class=\"card\">\r\n                        <div class=\"card-body\">\r\n                            <h4>Tambah Kelas</h4>\r\n                            <br>\r\n                            <form class=\"row g-3\" @submit.prevent=\"update()\">\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">Nama Kelas</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"kelas.nama_kelas\">\r\n                                    <div v-if=\"Validation.nama_kelas\" class=\"text-danger\">\r\n                                        {{Validation.nama_kelas[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">Mapel</label>\r\n                                    <select name=\"mapel\" class=\"form-select\" v-model=\"ujian.mapel_id\">\r\n                                        <option v-for=\"(mapel, index) in mapel.data\" :key=\"index\" :value=\"mapel.id_mapel\">{{mapel.nama_mapel}}</option>\r\n                                    </select>\r\n                                    <div v-if=\"Validation.mapel_id\" class=\"text-danger\">\r\n                                        {{Validation.mapel_id[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-12\">\r\n                                    <button class=\"btn btn-warning\" type=\"submit\">Update</button>\r\n                                </div>\r\n                            </form>  \r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer START -->\r\n                <div class=\"footer fixed-bottom\">\r\n                    <div class=\"footer-content\">\r\n                        <p class=\"mb-0\">Copyright Â© 2022 All rights reserved.</p>\r\n                        <span>\r\n                            <a href=\"\" class=\"text-gray me-3\">Term &amp; Conditions</a>\r\n                            <a href=\"\" class=\"text-gray\">Privacy &amp; Policy</a>\r\n                        </span>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer End -->\r\n            </div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive ,ref, onMounted} from 'vue'\r\nimport { useRoute, useRouter} from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    setup(){\r\n        //data binding\r\n        const kelas = reactive({\r\n            nama_kelas: '',\r\n            jurusan_id: '',\r\n        });\r\n\r\n        const Validation = ref([]);\r\n\r\n        const router = useRouter();\r\n        const route = useRoute();\r\n\r\n        onMounted(() => {\r\n            axios.get(`http://localhost:8000/api/kelas/${route.params.id}`)\r\n            .then((result) => {\r\n                kelas.nama_kelas = result.data.data.nama_kelas\r\n                kelas.jurusan_id = result.data.data.jurusan_id\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        function update() {\r\n            axios.put(\r\n                `http://localhost:8000/api/kelas/${route.params.id}`,\r\n                kelas\r\n            )\r\n             .then(() => {\r\n                router.push({\r\n                    name : 'kelas.index'\r\n                })\r\n            }).catch((err) => {\r\n                Validation.value = err.response.data\r\n            });\r\n        }\r\n\r\n        return {\r\n            kelas,\r\n            Validation,\r\n            router,\r\n            update\r\n        }\r\n    }    \r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}