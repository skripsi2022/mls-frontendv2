{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const kelas = reactive({\n      nama_kelas: '',\n      jurusan_id: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n\n    function store() {\n      axios.post('http://localhost:8000/api/kelas', kelas).then(() => {\n        router.push({\n          name: 'kelas.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    let jurusan = ref([]);\n    onMounted(() => {\n      //get jurusan from api\n      axios.get('http://localhost:8000/api/jurusan').then(result => {\n        jurusan.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n    return {\n      kelas,\n      jurusan,\n      Validation,\n      router,\n      store\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiEA,SAAQA,QAAR,EAAkBC,GAAlB,EAAsBC,SAAtB,QAAsC,KAAtC;AACA,SAASC,SAAT,QAAyB,YAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIN,QAAQ,CAAC;AACnBO,gBAAU,EAAE,EADO;AAEnBC,gBAAU,EAAE;AAFO,KAAD,CAAtB;AAKA,UAAMC,UAAS,GAAIR,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMS,MAAK,GAAIP,SAAS,EAAxB;;AAEA,aAASQ,KAAT,GAAiB;AACbP,WAAK,CAACQ,IAAN,CACI,iCADJ,EAEIN,KAFJ,EAIEO,IAJF,CAIO,MAAM;AACTH,cAAM,CAACI,IAAP,CAAY;AACRC,cAAG,EAAI;AADC,SAAZ;AAGH,OARD,EAQGC,KARH,CAQUC,GAAD,IAAS;AACdR,kBAAU,CAACS,KAAX,GAAmBD,GAAG,CAACE,QAAJ,CAAaC,IAAhC;AACH,OAVD;AAWJ;;AAEA,QAAIC,OAAM,GAAIpB,GAAG,CAAC,EAAD,CAAjB;AAEAC,aAAS,CAAC,MAAM;AACZ;AACAE,WAAK,CAACkB,GAAN,CAAU,mCAAV,EACCT,IADD,CACOU,MAAD,IAAY;AACdF,eAAO,CAACH,KAAR,GAAgBK,MAAM,CAACH,IAAvB;AACH,OAHD,EAGGJ,KAHH,CAGUC,GAAD,IAAS;AACdO,eAAO,CAACC,GAAR,CAAYR,GAAG,CAACE,QAAhB;AACH,OALD;AAMH,KARQ,CAAT;AAUA,WAAO;AACHb,WADG;AAEHe,aAFG;AAGHZ,gBAHG;AAIHC,YAJG;AAKHC;AALG,KAAP;AAOJ;;AA7CW,CAAf","names":["reactive","ref","onMounted","useRouter","axios","