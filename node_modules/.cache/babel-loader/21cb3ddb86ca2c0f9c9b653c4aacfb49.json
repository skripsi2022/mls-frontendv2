ntent-between\">\r\n                                        <div>\r\n                                            <h3>4 Soal</h3>\r\n                                            <span class=\"text-muted fw-semibold\">Total</span>\r\n                                        </div>\r\n                                        <div class=\"text-warning fw-bold font-size-lg\"><i class=\"icon-book-open feather\"></i></div>\r\n                                        </div>\r\n                                    <div class=\"resize-triggers\">\r\n                                        <div class=\"expand-trigger\">\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        <!-- <div class=\"col-lg-3\">\r\n                            <div class=\"card\">                            \r\n                                <div class=\"card-body\" style=\"position: relative;\">\r\n                                    <div class=\"d-flex align-items-center justify-content-between\">\r\n                                        <div>\r\n                                            <h3>$168.90</h3>\r\n                                            <span class=\"text-muted fw-semibold\">This Month</span>\r\n                                        </div>\r\n                                        <div class=\"text-success fw-bold font-size-lg\">+18%</div>\r\n                                        </div>\r\n                                    <div class=\"resize-triggers\">\r\n                                        <div class=\"expand-trigger\">\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div> -->\r\n                        <div class=\"col-lg-9\">\r\n                            <div class=\"\">                            \r\n                                <div class=\"card-body\" style=\"position: relative;\">\r\n                                     <div class=\"d-grid gap-2 d-md-flex justify-content-md-end\">\r\n                                        <router-link :to=\"{name: 'ujian.create'}\" class=\"btn btn-warning me-md-end\" type=\"button\"><i class=\"icon-plus-circle feather\"></i>Tambah</router-link>\r\n                                        <router-link :to=\"{name: 'ujian.index'}\" class=\"btn btn-success me-md-end\" type=\"button\"><i class=\"icon-arrow-left-circle feather\"></i>Kembali</router-link>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        \r\n                    </div>\r\n                     <div class=\"card\">\r\n                        <div class=\"card-body\">\r\n                            <h4>Detail Ujian</h4>\r\n                            <br>\r\n                            <form class=\"row g-3\">\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">Nama Ujian</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"ujian.nama_ujian\">\r\n                                    <div v-if=\"Validation.nama_ujian\" class=\"text-danger\">\r\n                                        {{Validation.nama_ujian[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                 <div class=\"col-md-4\">\r\n                                    <label for=\"\" class=\"form-label\">Mata Pelajaran</label>\r\n                                    <select name=\"guru\" class=\"form-select\" v-model=\"ujian.mapel_id\">\r\n                                        <option v-for=\"(mapel, index) in mapel.data\" :key=\"index\" :value=\"mapel.id_mapel\">{{mapel.nama_mapel}}</option>\r\n                                    </select>\r\n                                    <div v-if=\"Validation.mapel_id\" class=\"text-danger\">\r\n                                        {{Validation.mapel_id[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                 <div class=\"col-md-4\">\r\n                                    <label for=\"\" class=\"form-label\">Kelas</label>\r\n                                    <select name=\"guru\" class=\"form-select\" v-model=\"ujian.kelas_id\">\r\n                                        <option v-for=\"(kelas, index) in kelas.data\" :key=\"index\" :value=\"kelas.id_kelas\">{{kelas.nama_kelas}}</option>\r\n                                    </select>\r\n                                    <div v-if=\"Validation.kelas_id\" class=\"text-danger\">\r\n                                        {{Validation.kelas_id[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-12\">\r\n                                    <button class=\"btn btn-warning\" type=\"submit\">Update</button>\r\n                                </div>\r\n                            </form>  \r\n                        </div>\r\n                    </div>\r\n                    <div class=\"card\">\r\n                        <div class=\"card-body\">\r\n                            <h4>Ujian </h4>\r\n                            <div class=\"mt-4\">\r\n                                <table id=\"data-table\" class=\"table data-table\">\r\n                                    <thead>\r\n                                        <tr>\r\n                                            <th>Soal</th>                                           \r\n                                            <th>A</th>                                           \r\n                                            <th>B</th>                                           \r\n                                            <th>C</th>                                           \r\n                                            <th>D</th>                                           \r\n                                            <th>Jawaban</th>                                           \r\n                                            <!-- <th>Action</th> -->\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody>\r\n                                        <tr v-for=\"(soal, index) in soal.data\" :key=\"index\"> \r\n                                            <td>{{soal.isi_soal}}</td>\r\n                                            <td>{{soal.opsi_a}}</td>\r\n                                            <td>{{soal.opsi_b}}</td>\r\n                                            <td>{{soal.opsi_c}}</td>\r\n                                            <td>{{soal.opsi_d}}</td>\r\n                                            <td>{{soal.opsi_benar}}</td>\r\n                                            <!-- <td>\r\n                                                <router-link :to=\"{name: 'ujian.detail',params:{id:ujian.id_ujian}}\" class=\"btn btn-info btn-sm\">Detail</router-link>\r\n                                                <button class=\"btn btn-danger btn-sm\" @click.prevent=\"destroy(ujian.id_ujian, index)\">Delete</button>\r\n                                                <button class=\"btn btn-warning btn-sm\"><i class=\"icon-settings feather\"></i>Atur Soal</button>\r\n                                            </td> -->\r\n                                        </tr>                                                                                                 \r\n                                    </tbody>                                   \r\n                                </table>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer START -->\r\n                <div class=\"footer fixed-bottom \">\r\n                    <div class=\"footer-content justify-content-md-end\">\r\n                        <p class=\"mb-0\">Copyright Â© 2022. All rights reserved.</p>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer End -->\r\n            </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {reactive ,ref, onMounted} from 'vue'\r\nimport { useRoute,} from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    // \r\n    setup(){\r\n        //data binding\r\n        const ujian = reactive({\r\n            nama_ujian: '',\r\n            mapel_id: '',\r\n            kelas_id: '',\r\n        });\r\n\r\n        const route = useRoute();\r\n\r\n        onMounted(() => {\r\n            axios.get(`http://localhost:8000/api/ujian/${route.params.id}`)\r\n            .then((result) => {\r\n                ujian.nama_ujian = result.data.data.nama_ujian\r\n                ujian.mapel_id = result.data.data.mapel_id\r\n                ujian.kelas_id = result.data.data.kelas_id\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        let soal = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get soal from api\r\n            axios.get('http://localhost:8000/api/soal')\r\n            .then((result) => {\r\n                soal.value = result.data\r\n            }).catch((err) => {\r\n                console.log(err.response)\r\n            });\r\n        });\r\n\r\n        function destroy(id,index){\r\n            axios.delete(\r\n                `http://localhost:8000/api/soalsoal/${id}`\r\n            )\r\n            .then(() => {\r\n                soal.value.data.splice(index,1)\r\n            }).catch((err) => {\r\n                console.log(err.response.data);\r\n            });\r\n        } \r\n\r\n        return {\r\n            soal,destroy,\r\n        }\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}