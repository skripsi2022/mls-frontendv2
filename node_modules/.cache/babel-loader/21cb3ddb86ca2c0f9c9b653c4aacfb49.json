{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  // \n  setup() {\n    //data binding\n    const ujian = reactive({\n      nama_ujian: '',\n      mapel_id: '',\n      kelas_id: ''\n    });\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/ujian/${route.params.id}`).then(result => {\n        ujian.nama_ujian = result.data.data.nama_ujian;\n        ujian.mapel_id = result.data.data.mapel_id;\n        ujian.kelas_id = result.data.data.kelas_id;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let soal = ref([]);\n    onMounted(() => {\n      //get soal from api\n      axios.get('http://localhost:8000/api/soal').then(result => {\n        soal.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`http://localhost:8000/api/soalsoal/${id}`).then(() => {\n        soal.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      soal,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA8IA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,QAAyB,YAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACX;AACAC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIN,QAAQ,CAAC;AACnBO,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,cAAQ,EAAE;AAHS,KAAD,CAAtB;AAMA,UAAMC,KAAI,GAAIP,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZE,WAAK,CAACO,GAAN,CAAW,mCAAkCD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA7D,EACCC,IADD,CACOC,MAAD,IAAY;AACdT,aAAK,CAACC,UAAN,GAAmBQ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBT,UAApC;AACAD,aAAK,CAACE,QAAN,GAAiBO,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBR,QAAlC;AACAF,aAAK,CAACG,QAAN,GAAiBM,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBP,QAAlC;AACH,OALD,EAKGQ,KALH,CAKUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OAPD;A