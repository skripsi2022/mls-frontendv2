     });\n    }\n\n    function update() {\n      axios.put(`/api/ujian/${route.params.id}`, ujian).then(() => {\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    function destroy(id, index) {\n      axios.delete(`/api/soal/${id}`).then(() => {\n        soal.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      ujian,\n      mapel,\n      kelas,\n      soal,\n      store,\n      destroy,\n      update,\n      addsoal,\n      Validation,\n      router\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgLA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,MAAI,GAAE;AACF,WAAO,EAAP;AAGH,GALU;;AAMXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIR,QAAQ,CAAC;AACnBS,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,cAAQ,EAAE,EAHS;AAInBC,cAAQ,EAAE;AAJS,KAAD,CAAtB;AAOA,UAAMC,OAAM,GAAIb,QAAQ,CAAC;AACrBc,cAAQ,EAAE,EADW;AAErBC,YAAM,EAAE,EAFa;AAGrBC,YAAM,EAAE,EAHa;AAIrBC,YAAM,EAAE,EAJa;AAKrBC,YAAM,EAAE,EALa;AAMrBC,gBAAU,EAAE,EANS;AAOrBC,cAAQ,EAAE;AAPW,KAAD,CAAxB;AAUA,UAAMC,UAAS,GAAIpB,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMqB,MAAK,GAAIlB,SAAS,EAAxB;AACA,UAAMmB,KAAI,GAAIpB,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACmB,GAAN,CAAW,cAAaD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAAxC,EACCC,IADD,CACOC,MAAD,IAAY;AACdpB,aAAK,CAACE,QAAN,GAAiBkB,MAAM,CAACtB,IAAP,CAAYA,IAAZ,CAAiBI,QAAlC;AACAF,aAAK,CAACC,UAAN,GAAmBmB,MAAM,CAACtB,IAAP,CAAYA,IAAZ,CAAiBG,UAApC;AACAD,aAAK,CAACG,QAAN,GAAiBiB,MAAM,CAACtB,IAAP,CAAYA,IAAZ,CAAiBK,QAAlC;AACAH,aAAK,CAACI,QAAN,GAAiBgB,MAAM,CAACtB,IAAP,CAAYA,IAAZ,CAAiBM,QAAlC;AACH,OAND,EAMGiB,KANH,CAMUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAa3B,IAAzB;AACH,OARD;AASH,KAVQ,CAAT;AAYA,QAAI4B,KAAI,GAAIjC,GAAG,CAAC,EAAD,CAAf;AAEAC,aAAS,CAAC,MAAM;AACZ;AACAG,WAAK,CAACmB,GAAN,CAAU,YAAV,EACCG,IADD,CACOC,MAAD,IAAY;AACdM,a