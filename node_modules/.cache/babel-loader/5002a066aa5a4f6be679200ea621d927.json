{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  methods: {\n    ujianges() {\n      // console.log(\"kembali ges\");\n      console.log(\"local storage dari tambah :\" + localStorage.ujian_id);\n      localStorage.setItem(\"ujian_id\", ujian.id_ujian); // localStorage.removeItem('ujian_id'); \n      // window.history.back()\n    }\n\n  },\n\n  setup() {\n    //data binding\n    const ujian = reactive({\n      nama_ujian: '',\n      id_ujian: '',\n      mapel: '',\n      kelas: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`/api/ujian/${route.params.id}`).then(result => {\n        // localStorage.removeItem(\"ujian_id\")\n        ujian.id_ujian = result.data.data.id_ujian;\n        ujian.nama_ujian = result.data.data.nama_ujian;\n        ujian.mapel = result.data.data.mapel.nama_mapel;\n        ujian.kelas = result.data.data.kelas.nama_kelas; //set localStorage\n\n        localStorage.setItem(\"ujian_id\", ujian.id_ujian);\n        console.log(\"local storage dari ujian :\" + localStorage.ujian_id);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let id_ujian = localStorage.ujian_id;\n    const addsoal = reactive({\n      isi_soal: '',\n      opsi_a: '',\n      opsi_b: '',\n      opsi_c: '',\n      opsi_d: '',\n      opsi_benar: '',\n      ujian_id: id_ujian\n    });\n    let soal = ref([]);\n    onMounted(() => {\n      //get mapel from api\n      axios.get(`/api/getSoalByUjian/${route.params.id}`).then(result => {\n        soal.value = result.data;\n        console.log(\"local storage dari soal :\" + localStorage.ujian_id);\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function back() {\n      console.log(\"ashiap\");\n    }\n\n    function store() {\n      console.log(\"local storage dari store : \" + addsoal.ujian_id); // window.history.back()\n      // axios.post(\n      //     '/api/soal',\n      //     addsoal\n      // )\n      //  .then(() => {\n      //     localStorage.removeItem('ujian_id');\n      //     router.push({\n      //         name : 'ujian.index'\n      //     })\n      // }).catch((err) => {\n      //     Validation.value = err.response.data\n      // });\n    }\n\n    function destroy(id, index) {\n      axios.delete(`/api/soal/${id}`).then(() => {\n        soal.value.data.splice(index, 1);\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      ujian,\n      soal,\n      back,\n      store,\n      destroy,\n      addsoal,\n      Validation,\n      router\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiLA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,SAAM,EAAI;AACNC,YAAQ,GAAE;AACN;AACAC,aAAO,CAACC,GAAR,CAAY,gCAAgCC,YAAY,CAACC,QAAzD;AACAD,kBAAY,CAACE,OAAb,CAAqB,UAArB,EAAiCC,KAAK,CAACC,QAAvC,EAHM,CAIN;AACA;AACJ;;AAPM,GADC;;AAWXC,OAAK,GAAE;AAGH;AACA,UAAMF,KAAI,GAAIb,QAAQ,CAAC;AACnBgB,gBAAU,EAAE,EADO;AAEnBF,cAAQ,EAAE,EAFS;AAGnBG,WAAK,EAAE,EAHY;AAInBC,WAAK,EAAE;AAJY,KAAD,CAAtB;AAQA,UAAMC,UAAS,GAAIlB,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMmB,MAAK,GAAIhB,SAAS,EAAxB;AACA,UAAMiB,KAAI,GAAIlB,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACiB,GAAN,CAAW,cAAaD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAAxC,EACCC,IADD,CACOC,MAAD,IAAY;AACd;AAEAb,aAAK,CAACC,QAAN,GAAiBY,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBb,QAAlC;AACAD,aAAK,CAACG,UAAN,GAAmBU,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBX,UAApC;AACAH,aAAK,CAACI,KAAN,GAAcS,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBV,KAAjB,CAAuBW,UAArC;AACAf,aAAK,CAACK,KAAN,GAAcQ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBT,KAAjB,CAAuBW,UAArC,CANc,CAQd;;AACAnB,oBAAY,CAACE,OAAb,CAAqB,UAArB,EAAiCC,KAAK,CAACC,QAAvC;AAEAN,eAAO,CAACC,GAAR,CAAY,+