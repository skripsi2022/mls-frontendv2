\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Soal\\IndexView.vue"],"sourcesContent":["<template>\n    <div class=\"content container\">\n        <div class=\"main\">\n            <div class=\"row\">\n                <div class=\"col-lg-8\">\n                    <div class=\"page-header\">\n                        <h4 class=\"page-title\">Ujian</h4>\n                        <div class=\"breadcrumb\">\n                            <span class=\"me-1 text-gray\"><i class=\"feather icon-home\"></i></span>\n                            <div class=\"breadcrumb-item\">Home</div>\n                            <div class=\"breadcrumb-item\">Ujian</div>\n                            <div class=\"breadcrumb-item\">Detail</div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-lg-4\">\n                    <div class=\"d-grid d-md-flex justify-content-md-end\">\n                        <router-link :to=\"{name: 'ujian.index'}\" class=\"btn btn-warning me-md-end\" type=\"button\"><i class=\"icon-arrow-left-circle feather\"></i>Kembali</router-link>\n                        <!-- <button class=\"btn btn-warning me-md-end\"><i class=\"icon-arrow-left-circle feather\"  @click=\"back()\"></i>Kembali</button> -->\n                        <button class=\" btn btn-primary me-md-end\" data-bs-toggle=\"modal\"  data-bs-target=\"#exampleModalCenter\" @click=\"ujianges()\">Tambah</button>\n                    </div>\n                </div>\n            </div>\n            <br>\n            <div class=\"card\">\n                <div class=\"card-body\">\n                    <h4>Detail Ujian</h4>\n                    <br>\n                    <form class=\"row g-3\">\n                        <div class=\"col-md-6\">\n                            <label for=\"\" class=\"form-label\">Nama Ujian</label>\n                            <input type=\"text\" class=\"form-control\" v-model=\"ujian.nama_ujian\" disabled=\"\">\n                            <div v-if=\"Validation.nama_ujian\" class=\"text-danger\">\n                                {{Validation.nama_ujian[0]}}\n                            </div>\n                        </div>\n                        <div class=\"col-md-4\">\n                            <label for=\"\" class=\"form-label\">Mata Pelajaran</label>\n                            <input type=\"text\" class=\"form-control\" v-model=\"ujian.mapel\" disabled=\"\">\n                            <div v-if=\"Validation.mapel\" class=\"text-danger\">\n                                {{Validation.mapel[0]}}\n                            </div>\n                        </div>\n                        <div class=\"col-md-4\">\n                            <label for=\"\" class=\"form-label\">Kelas</label>\n                            <input type=\"text\" class=\"form-control\" v-model=\"ujian.kelas\" disabled=\"\">\n                            <div v-if=\"Validation.kelas_id\" class=\"text-danger\">\n                                {{Validation.kelas_id[0]}}\n                            </div>\n                        </div>\n                    </form>\n                </div>\n            </div>\n            <div class=\"card\">\n                <div class=\"card-body\">\n                    <h4>Soal <span class=\"lead\">{{ujian.nama_ujian}}</span> </h4>\n                    <div class=\"mt-4\">\n                        <table id=\"data-table\" class=\"table data-table\">\n                            <thead>\n                                <tr>\n                                    <th>No.</th>\n                                    <th>Soal</th>\n                                    <th>A</th>\n                                    <th>B</th>\n                                    <th>C</th>\n                                    <th>D</th>\n                                    <th>Jawaban</th>\n                                    <th>Action</th>\n                                </tr>\n                            </thead>\n                            <tbody>\n                                <tr v-for=\"(soal, index) in soal.data\" :key=\"index\">\n                                    <td>{{++index}}</td>\n                                    <td>{{soal.isi_soal}}</td>\n                                    <td>{{soal.opsi_a}}</td>\n                                    <td>{{soal.opsi_b}}</td>\n                                    <td>{{soal.opsi_c}}</td>\n                                    <td>{{soal.opsi_d}}</td>\n                                    <td>{{soal.opsi_benar}}</td>\n                                    <td>\n                                        <!-- <router-link :to=\"{name: 'ujian.detail',params:{id:soal.id_soal}}\"\n                                            class=\"btn btn-info btn-sm\">Detail</router-link> -->\n                                        <button class=\"btn btn-danger btn-sm\"\n                                            @click.prevent=\"destroy(soal.id_soal, index)\">Delete</button>\n                                    </td>\n                                </tr>\n                            </tbody>\n                        </table>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"modal fade\" id=\"exampleModalCenter\" tabindex=\"-1\" aria-labelledby=\"exampleModalCenterLabel\"\n            aria-hidden=\"true\">\n            <div class=\"modal-dialog modal-dialog-centered\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-header\">\n                        <h5 class=\"modal-title\" id=\"exampleModalCenterLabel\">Tambah Soal</h5>\n                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                    </div>\n                    <div class=\"card\">\n                        <div class=\"card-body\">\n                            <form class=\"row g-3\" @submit.prevent=\"store()\">\n                                <div class=\"col-md-12\">\n                                    <label for=\"\" class=\"form-label\">Isi Soal</label>\n                                    <textarea type=\"text\" class=\"form-control\" v-model=\"addsoal.isi_soal\"></textarea>\n                                    <div v-if=\"Validation.isi_soal\" class=\"text-danger\">\n                                        {{Validation.isi_soal[0]}}\n                                    </div>\n                                </div>\n                                <div class=\"col-md-6\">\n                                    <label for=\"\" class=\"form-label\">A</label>\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_a\">\n                                    <div v-if=\"Validation.opsi_a\" class=\"text-danger\">\n                                        {{Validation.opsi_a[0]}}\n                                    </div>\n                                </div>\n                                <div class=\"col-md-6\">\n                                    <label for=\"\" class=\"form-label\">B</label>\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_b\">\n                                    <div v-if=\"Validation.opsi_b\" class=\"text-danger\">\n                                        {{Validation.opsi_b[0]}}\n                                    </div>\n                                </div>\n                                <div class=\"col-md-6\">\n                                    <label for=\"\" class=\"form-label\">C</label>\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_c\">\n                                    <div v-if=\"Validation.opsi_c\" class=\"text-danger\">\n                                        {{Validation.opsi_c[0]}}\n                                    </div>\n                                </div>\n                                <div class=\"col-md-6\">\n                                    <label for=\"\" class=\"form-label\">D</label>\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_d\">\n                                    <div v-if=\"Validation.opsi_d\" class=\"text-danger\">\n                                        {{Validation.opsi_d[0]}}\n                                    </div>\n                                </div>\n                                <div class=\"col-md-6\">\n                                    <label for=\"\" class=\"form-label\">Jawaban Benar</label>\n                                    <select name=\"soal\" class=\"form-select\" v-model=\"addsoal.opsi_benar\">\n                                        <option value=\"opsi_a\">A</option>\n                                        <option value=\"opsi_b\">B</option>\n                                        <option value=\"opsi_c\">C</option>\n                                        <option value=\"opsi_d\">D</option>\n                                    </select>\n                                    <div v-if=\"Validation.opsi_benar\" class=\"text-danger\">\n                                        {{Validation.opsi_benar[0]}}\n                                    </div>\n                                </div>\n                                <!-- <div class=\"col-md-6\">\n                                                            <label for=\"\" class=\"form-label\">Ujian ID</label>\n                                                            <input type=\"text\" class=\"form-control\" v-model=\"addsoal.ujian_id\">\n                                                            <div v-if=\"Validation.ujian_id\" class=\"text-danger\">\n                                                                {{Validation.ujian_id[0]}}\n                                                            </div>\n                                                        </div>                                                           -->\n                                <div class=\"col-12\">\n                                    <button class=\"btn btn-primary\" type=\"submit\">Tambah</button>\n                                </div>\n                            </form>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <!-- Footer START -->\n        <!-- <div class=\"footer fixed-bottom \">\n                    <div class=\"footer-content justify-content-md-end\">\n                        <p class=\"mb-0\">Copyright © 2022. All rights reserved.</p>\n                    </div>\n                </div> -->\n        <!-- Footer End -->\n    </div>\n</template>\n\n<script>\nimport {reactive ,ref, onMounted} from 'vue'\nimport { useRoute, useRouter} from 'vue-router'\nimport axios from 'axios'\n\nexport default {\n    methods : {\n        ujianges(){\n            // console.log(\"kembali ges\");\n            console.log(\"local storage dari tambah :\" + localStorage.ujian_id)\n            localStorage.setItem(\"ujian_id\", ujian.id_ujian)\n            // localStorage.removeItem('ujian_id'); \n            // window.history.back()\n        }\n    },\n\n    setup(){\n       \n\n        //data binding\n        const ujian = reactive({\n            nama_ujian: '',\n            id_ujian: '',\n            mapel: '',\n            kelas: '',\n        });\n\n        \n        const Validation = ref([]);\n\n        const router = useRouter();\n        const route = useRoute();\n\n        onMounted(() => {\n            axios.get(`/api/ujian/${route.params.id}`)\n            .then((result) => {\n                // localStorage.removeItem(\"ujian_id\")\n\n                ujian.id_ujian = result.data.data.id_ujian\n                ujian.nama_ujian = result.data.data.nama_ujian\n                ujian.mapel = result.data.data.mapel.nama_mapel\n                ujian.kelas = result.data.data.kelas.nama_kelas\n                \n                //set localStorage\n                localStorage.setItem(\"ujian_id\", ujian.id_ujian)\n                \n                console.log(\"local storage dari ujian :\" + localStorage.ujian_id)\n\n            }).catch((err) => {\n                console.log(err.response.data)\n            });\n            \n        });\n\n        let id_ujian = localStorage.ujian_id;\n\n        const addsoal = reactive({\n            isi_soal: '',\n            opsi_a: '',\n            opsi_b: '',\n            opsi_c: '',\n            opsi_d: '',\n            opsi_benar: '',\n            ujian_id: id_ujian,\n        });\n\n        let soal = ref([]);\n\n        onMounted(() => {\n            //get mapel from api\n            axios.get(`/api/getSoalByUjian/${route.params.id}`)\n            .then((result) => {\n                soal.value = result.data\n                console.log(\"local storage dari soal :\" + localStorage.ujian_id)\n            }).catch((err) => {\n                console.log(err.response)\n            });\n        });\n\n    \n        function back(){\n            console.log(\"ashiap\")\n        }\n\n        function store() {\n            console.log(\"local storage dari store : \" + addsoal.ujian_id)\n            // window.history.back()\n\n            // axios.post(\n            //     '/api/soal',\n            //     addsoal\n            // )\n            //  .then(() => {\n            //     localStorage.removeItem('ujian_id');\n            //     router.push({\n            //         name : 'ujian.index'\n            //     })\n                \n            // }).catch((err) => {\n            //     Validation.value = err.response.data\n            // });\n        }\n\n\n        function destroy(id,index){\n            axios.delete(\n                `/api/soal/${id}`\n            )\n            .then(() => {\n                soal.value.data.splice(index,1)\n\n                router.push({\n                    name: 'ujian.index'\n                })\n            }).catch((err) => {\n                console.log(err.response.data);\n            });\n        } \n\n        return {\n            ujian,\n            soal,\n            back,\n            store,destroy,\n            addsoal,\n            Validation,\n            router,\n        }\n    }    \n}\n</script>"]},"metadata":{},"sourceType":"module"}