{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const soal = reactive({\n      isi_soal: '',\n      opsi_a: '',\n      opsi_b: '',\n      opsi_c: '',\n      opsi_d: '',\n      opsi_benar: '',\n      ujian_id: ''\n    });\n    const ujian = reactive({\n      id_ujian: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/ujian/${route.params.id}`).then(result => {\n        ujian.id_ujian = result.data.data.id_ujian;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n\n    function store() {\n      axios.post('http://localhost:8000/api/soal', soal).then(() => {\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      soal,\n      Validation,\n      router,\n      store\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmGA,SAAQA,QAAR,EAAkBC,GAAlB,EAAsBC,SAAtB,QAAsC,KAAtC;AACA,SAAQC,QAAR,EAAiBC,SAAjB,QAAiC,YAAjC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,IAAG,GAAIP,QAAQ,CAAC;AAClBQ,cAAQ,EAAE,EADQ;AAElBC,YAAM,EAAE,EAFU;AAGlBC,YAAM,EAAE,EAHU;AAIlBC,YAAM,EAAE,EAJU;AAKlBC,YAAM,EAAE,EALU;AAMlBC,gBAAU,EAAE,EANM;AAOlBC,cAAQ,EAAE;AAPQ,KAAD,CAArB;AAUA,UAAMC,KAAI,GAAIf,QAAQ,CAAC;AACnBgB,cAAQ,EAAE;AADS,KAAD,CAAtB;AAIA,UAAMC,UAAS,GAAIhB,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMiB,MAAK,GAAId,SAAS,EAAxB;AACA,UAAMe,KAAI,GAAIhB,QAAQ,EAAtB;AAECD,aAAS,CAAC,MAAM;AACbG,WAAK,CAACe,GAAN,CAAW,mCAAkCD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA7D,EACCC,IADD,CACOC,MAAD,IAAY;AACdT,aAAK,CAACC,QAAN,GAAiBQ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBT,QAAlC;AACH,OAHD,EAGGU,KAHH,CAGUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OALD;AAMH,KAPS,CAAT;;AA