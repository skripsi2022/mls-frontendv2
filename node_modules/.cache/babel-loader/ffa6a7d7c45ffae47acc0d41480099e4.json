{"ast":null,"code":"import { reactive, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const jurusan = reactive({\n      nama_jurusan: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n\n    function store() {\n      axios.post('http://localhost:8000/api/jurusan', jurusan).then(() => {\n        router.push({\n          name: 'siswa'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      jurusan,\n      Validation,\n      router,\n      store\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgFA,SAAQA,QAAR,EAAkBC,GAAlB,QAA4B,KAA5B;AACA,SAASC,SAAT,QAAyB,YAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,OAAM,GAAIL,QAAQ,CAAC;AACrBM,kBAAY,EAAE;AADO,KAAD,CAAxB;AAIA,UAAMC,UAAS,GAAIN,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMO,MAAK,GAAIN,SAAS,EAAxB;;AAEA,aAASO,KAAT,GAAiB;AACbN,WAAK,CAACO,IAAN,CACI,mCADJ,EAEIL,OAFJ,EAIEM,IAJF,CAIO,MAAM;AACTH,cAAM,CAACI,IAAP,CAAY;AACRC,cAAG,EAAI;AADC,SAAZ;AAGH,OARD,EAQGC,KARH,CAQUC,GAAD,IAAS;AACdR,kBAAU,CAACS,KAAX,GAAmBD,GAAG,CAACE,QAAJ,CAAaC,IAAhC;AACH,OAVD;AAWJ;;AAEA,WAAO;AACHb,aADG;AAEHE,gBAFG;AAGHC,YAHG;AAIHC;AAJG,KAAP;AAMJ;;AA/BW,CAAf","names":["reactive","ref","useRouter","axios","setup","jurusan","nama_jurusan","Validation","router","store","post","then","push","name","catch","err","value","response","data"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Siswa\\CreateView.vue"],"sourcesContent":["<template>\r\n     <div class=\"content container\">\r\n                <div class=\"main\">\r\n                    <div class=\"row\">\r\n                        <div class=\"col-lg-8\">\r\n                            <div class=\"page-header\">\r\n                                <h4 class=\"page-title\">Siswa</h4>\r\n                                <div class=\"brea