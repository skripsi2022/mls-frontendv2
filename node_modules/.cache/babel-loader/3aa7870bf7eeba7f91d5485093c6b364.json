  <tbody>\r\n                                <tr v-for=\"(ujian, index) in ujian.data\" :key=\"index\">\r\n                                    <td>{{++index}}</td>\r\n                                    <td>{{ujian.nama_ujian}}</td>\r\n                                    <td>{{ujian.mapel.nama_mapel}}</td>\r\n                                    <td>{{ujian.kelas.nama_kelas}}</td>\r\n                                    <td>\r\n                                        <router-link :to=\"{name: 'ujian.detail',params:{id:ujian.id_ujian}}\" class=\"btn btn-info btn-sm\">Detail</router-link>\r\n                                        <button class=\"btn btn-danger btn-sm\" @click.prevent=\"destroy1(ujian.id_ujian, index)\">Delete</button>\r\n                                        <router-link :to=\"{name: 'soal.index', params:{id:ujian.id_ujian}}\" class=\"btn btn-warning btn-sm\"><i class=\"icon-settings feather\"></i>Atur Soal</router-link>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- Footer START -->\r\n        <!-- <div class=\"footer fixed-bottom \">\r\n            <div class=\"footer-content justify-content-md-end\">\r\n                <p class=\"mb-0\">Copyright Â© 2022. All rights reserved.</p>\r\n            </div>\r\n        </div> -->\r\n        <!-- Footer End -->\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\nimport {onMounted, ref} from 'vue'\r\n\r\nexport default {\r\n    \r\n     methods : {\r\n        destroy1(id){\r\n             Swal.fire({\r\n                 title: 'Are you sure?',\r\n                 text: \"You won't be able to revert this!\",\r\n                 icon: 'warning',\r\n                 showCancelButton: true,\r\n                 confirmButtonColor: '#3085d6',\r\n                 cancelButtonColor: '#d33',\r\n                 confirmButtonText: 'Yes, delete it!'\r\n             }).then((result) => {\r\n                 if (result.isConfirmed) {\r\n                     Swal.fire(\r\n                         'Deleted!',\r\n                         'Your file has been deleted.',\r\n                         'success'\r\n                     )\r\n                 }\r\n             })\r\n             axios.delete(\r\n                 `/api/ujian/${id}`\r\n             )\r\n                 .then(() => {\r\n                    //  ujian.value.data.splice(index, 1)           \r\n                     this.$swal({\r\n                        title: 'Success!',\r\n                        text: 'Data Ujian Berhasil dihapus !',\r\n                        icon: 'success',\r\n                        confirmButtonText: 'Lanjut !'\r\n                    });\r\n                 }).catch((err) => {\r\n                     console.log(err.response.data);\r\n                     this.$swal({\r\n                        title: 'error!',\r\n                        text: 'Kosongkan Soal terlebih dahulu !',\r\n                        icon: 'error',\r\n                        confirmButtonText: 'Lanjut !'\r\n                    });\r\n                 });\r\n        },\r\n    },\r\n    setup(){\r\n        let ujian = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get ujian from api\r\n               console.log(\"ujian id : \" + localStorage.ujian_id)\r\n            axios.get('/api/ujian')\r\n            .then((result) => {\r\n                ujian.value = result.data\r\n\r\n            }).catch((err) => {\r\n                console.log(err.response)\r\n            });\r\n        });\r\n\r\n        function destroy(id,index){\r\n            axios.delete(\r\n                `/api/ujian/${id}`\r\n            )\r\n            .then(() => {\r\n                ujian.value.data.splice(index,1)\r\n                window.location.reload();\r\n            }).catch((err) => {\r\n                console.log(err.response.data);\r\n            });\r\n        } \r\n\r\n        return {\r\n            ujian,destroy,\r\n        }\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}