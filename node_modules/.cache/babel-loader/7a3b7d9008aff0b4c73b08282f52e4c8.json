{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const kelas = reactive({\n      nama_kelas: '',\n      jurusan_id: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/kelas/${route.params.id}`).then(result => {\n        kelas.nama_kelas = result.data.data.nama_kelas;\n        kelas.jurusan_id = result.data.data.jurusan_id;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let jurusan = ref([]);\n    onMounted(() => {\n      //get jurusan from api\n      axios.get('http://localhost:8000/api/jurusan').then(result => {\n        jurusan.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n    let siswa = ref([]);\n    onMounted(() => {\n      //get siswa from api\n      axios.get('http://localhost:8000/api/siswa/').then(result => {\n        siswa.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function update() {\n      axios.put(`http://localhost:8000/api/kelas/${route.params.id}`, kelas).then(() => {\n        router.push({\n          name: 'kelas.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    filt;\n    return {\n      kelas,\n      siswa,\n      jurusan,\n      Validation,\n      router,\n      update\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsFA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIP,QAAQ,CAAC;AACnBQ,gBAAU,EAAE,EADO;AAEnBC,gBAAU,EAAE;AAFO,KAAD,CAAtB;AAKA,UAAMC,UAAS,GAAIT,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMU,MAAK,GAAIP,SAAS,EAAxB;AACA,UAAMQ