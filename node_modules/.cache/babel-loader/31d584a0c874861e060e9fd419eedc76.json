\r\n                router.push({\r\n                    name : 'mapel'\r\n                })\r\n            }).catch((err) => {\r\n                Validation.value = err.response.data\r\n            });\r\n        }\r\n        let guru = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get siswa from api\r\n            axios.get('http://localhost:8000/api/guru')\r\n            .then((result) => {\r\n                guru.value = result.data\r\n            }).catch((err) => {\r\n                console.log(err.response)\r\n            });\r\n        });\r\n\r\n        return {\r\n            mapel,\r\n            guru,\r\n            Validation,\r\n            router,\r\n            store\r\n        }\r\n    }    \r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}