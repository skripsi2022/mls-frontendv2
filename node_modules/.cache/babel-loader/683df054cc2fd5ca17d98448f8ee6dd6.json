{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  methods: {\n    showAlert() {\n      // Use sweetalert2\n      // this.$swal('Hello Vue world!!!');\n      vue.$swal('Hello Vue world!!!');\n    }\n\n  },\n\n  data() {\n    return {\n      //state loggedIn with localStorage\n      loggedIn: localStorage.getItem('loggedIn'),\n      //state token\n      token: localStorage.getItem('token')\n    };\n  },\n\n  setup() {\n    let mapel = ref([]);\n    onMounted(() => {\n      //get mapel by guru\n      axios.get('/api/mapelGuru', {\n        params: {\n          id: localStorage.getItem('id')\n        }\n      }).then(result => {\n        mapel.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`/api/mapel/${id}`).then(() => {\n        mapel.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      mapel,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACXC,SAAO,EAAE;AACLC,aAAS,GAAG;AACR;AACA;AACAC,SAAG,CAACC,KAAJ,CAAU,oBAAV;AACH;;AALI,GADE;;AASVC,MAAI,GAAG;AACJ,WAAO;AACH;AACAC,cAAQ,EAAEC,YAAY,CAACC,OAAb,CAAqB,UAArB,CAFP;AAGH;AACAC,WAAK,EAAEF,YAAY,CAACC,OAAb,CAAqB,OAArB;AAJJ,KAAP;AAMH,GAhBU;;AAkBXE,OAAK,GAAE;AACH,QAAIC,KAAI,GAAIX,GAAG,CAAC,EAAD,CAAf;AAEAD,aAAS,CAAC,MAAM;AACZ;AACAD,WAAK,CAACc,GAAN,CAAU,gBAAV,EAA2B;AACvBC,cAAM,EAAE;AACJC,YAAE,EAAEP,YAAY,CAACC,OAAb,CAAqB,IAArB;AADA;AADe,OAA3B,EAKCO,IALD,CAKOC,MAAD,IAAY;AACdL,aAAK,CAACM,KAAN,GAAcD,MAAM,CAACX,IAArB;AAEH,OARD,EAQGa,KARH,CAQUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAhB;AACH,OAVD;AAWH,KAbQ,CAAT;;AAeA,aAASC,OAAT,CAAiBT,EAAjB,EAAoBU,KAApB,EAA0B;AACtB1B,WAAK,CAAC2B,MAAN,CACK,cAAaX,EAAG,EADrB,EAGCC,IAHD,CAGM,MAAM;AACRJ,aAAK,CAACM,KAAN,CAAYZ,IAAZ,CAAiBqB,MAAjB,CAAwBF,KAA