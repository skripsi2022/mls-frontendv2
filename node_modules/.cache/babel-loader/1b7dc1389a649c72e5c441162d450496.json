{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    let ujian_id = localStorage.ujian_id; //data binding\n\n    const ujian = reactive({\n      nama_ujian: '',\n      id_ujian: '',\n      mapel: '',\n      kelas: ''\n    });\n    const addsoal = reactive({\n      isi_soal: '',\n      opsi_a: '',\n      opsi_b: '',\n      opsi_c: '',\n      opsi_d: '',\n      opsi_benar: '',\n      ujian_id: ujian_id\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`/api/ujian/${route.params.id}`).then(result => {\n        ujian.id_ujian = result.data.data.id_ujian;\n        ujian.nama_ujian = result.data.data.nama_ujian;\n        ujian.mapel = result.data.data.mapel.nama_mapel;\n        ujian.kelas = result.data.data.kelas.nama_kelas; //set localStorage\n\n        localStorage.setItem(\"ujian_id\", result.data.data.id_ujian);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let soal = ref([]);\n    onMounted(() => {\n      //get mapel from api\n      axios.get(`/api/getSoalByUjian/${route.params.id}`).then(result => {\n        soal.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function store() {\n      console.log(addsoal);\n      axios.post('/api/soal', addsoal).then(() => {\n        localStorage.removeItem('ujian_id');\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    function destroy(id, index) {\n      axios.delete(`/api/soal/${id}`).then(() => {\n        soal.value.data.splice(index, 1);\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      ujian,\n      soal,\n      store,\n      destroy,\n      addsoal,\n      Validation,\n      router\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmLA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AAEXC,OAAK,GAAE;AACH,QAAIC,QAAO,GAAIC,YAAY,CAACD,QAA5B,CADG,CAEH;;AACA,UAAME,KAAI,GAAIT,QAAQ,CAAC;AACnBU,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,WAAK,EAAE,EAHY;AAInBC,WAAK,EAAE;AAJY,KAAD,CAAtB;AAOA,UAAMC,OAAM,GAAId,QAAQ,CAAC;AACrBe,cAAQ,EAAE,EADW;AAErBC,YAAM,EAAE,EAFa;AAGrBC,YAAM,EAAE,EAHa;AAIrBC,YAAM,EAAE,EAJa;AAKrBC,YAAM,EAAE,EALa;AAMrBC,gBAAU,EAAE,EANS;AAOrBb,cAAQ,EAAEA;AAPW,KAAD,CAAxB;AAUA,UAAMc,UAAS,GAAIpB,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMqB,MAAK,GAAIlB,SAAS,EAAxB;AACA,UAAMmB,KAAI,GAAIpB,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACmB,GAAN,CAAW,cAAaD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAAxC,EACCC,IADD,CACOC,MAAD,IAAY;AACdnB,aAAK,CAACE,QAAN,GAAiBiB,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBlB,QAAlC;AACAF,aAAK,CAACC,UAAN,GAAmBkB,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBnB,UAApC;AACAD,aAAK,CAACG,KAAN,GAAcgB,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBjB,KAAjB,CAAuBkB,UAArC;AACArB,aAAK,CAACI,KAAN,GAAce,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBhB,KAAjB,CAAuBkB,UAArC,CAJc,CAKd;;AACAvB,oBAAY,CAACwB,OAAb,CAAqB,UAArB,EAAiCJ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBlB,QAAlD;AAEH,OATD,EASGsB,KATH,CASUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaR,IAAzB;AACH,OAXD;AAYH,KAbQ,CAAT;AAeA,QAAIS,IAAG,GAAIrC,GAAG,CAAC,EAAD,CAAd;AAEAC,aAAS,CAAC,MAAM;AACZ;AACAG,WAAK,CAACmB,GAAN,CAAW,uBAAsBD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAAjD,EACCC,IADD,CACOC,MAAD,IAAY;AACdU,YAAI,CAACC,KAAL,GAAaX,MAAM,CAACC,IAApB;AACH,OAHD,EAGGI,KAHH,CAGUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAhB;AACH,OALD;AAMH,KARQ,CAAT;;AAWA,aAASG,KAAT,GAAiB;AACbL,aAAO,CAACC,GAAR,CAAYtB,OAAZ;AACAT,WAAK,CAACoC,IAAN,CACI,WADJ,EAEI3B,OAFJ,EAIEa,IAJF,CAIO,MAAM;AACTnB,oBAAY,CAACkC,UAAb,CAAwB,UAAxB;AACApB,cAAM,CAACqB,IAAP,CAAY;AACRC,cAAG,EAAI;AADC,SAAZ;AAIH,OAVD,EAUGX,KAVH,CAUUC,GAAD,IAAS;AACdb,kBAAU,CAACkB,KAAX,GAAmBL,GAAG,CAACG,QAAJ,CAAaR,IAAhC;AACH,OAZD;AAaJ;;AAGA,aAASgB,OAAT,CAAiBnB,EAAjB,EAAoBoB,KAApB,EAA0B;AACtBzC,WAAK,CAAC0C,MAAN,CACK,aAAYrB,EAAG,EADpB,EAGCC,IAHD,CAGM,MAAM;AACRW,YAAI,CAACC,KAAL,CAAWV,IAAX,CAAgBmB,MAAhB,CAAuBF,KAAvB,EAA6B,CAA7B;AACAxB,cAAM,CAACqB,IAAP,CAAY;AACRC,cAAI,EAAE;AADE,SAAZ;AAGH,OARD,EAQGX,KARH,CAQUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaR,IAAzB;AACH,OAVD;AAWJ;;AAEA,WAAO;AACHpB,WADG;AAEH6B,UAFG;AAGHE,WAHG;AAGGK,aAHH;AAIH/B,aAJG;AAKHO,gBALG;AAMHC;AANG,KAAP;AAQJ;;AA/FW,CAAf","names":["reactive","ref","onMounted","useRoute","useRouter","axios","setup","ujian_id","localStorage","ujian","nama_ujian","id_ujian","mapel","kelas","addsoal","isi_soal","opsi_a","opsi_b","opsi_c","opsi_d","opsi_benar","Validation","router","route","get","params","id","then","result","data","nama_mapel","nama_kelas","setItem","catch","err","console","log","response","soal","value","store","post","removeItem","push","name","destroy","index","delete","splice"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Soal\\IndexView.vue"],"sourcesContent":["<template>\r\n    <div class=\"content container\">\r\n        <div class=\"main\">\r\n            <div class=\"row\">\r\n                <div class=\"col-lg-8\">\r\n                    <div class=\"page-header\">\r\n                        <h4 class=\"page-title\">Ujian</h4>\r\n                        <div class=\"breadcrumb\">\r\n                            <span class=\"me-1 text-gray\"><i class=\"feather icon-home\"></i></span>\r\n                            <div class=\"breadcrumb-item\">Home</div>\r\n                            <div class=\"breadcrumb-item\">Ujian</div>\r\n                            <div class=\"breadcrumb-item\">Detail</div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"col-lg-4\">\r\n                    <div class=\"d-grid d-md-flex justify-content-md-end\">\r\n                        <router-link :to=\"{name: 'ujian.index'}\" class=\"btn btn-warning me-md-end\" type=\"button\"><i\r\n                                class=\"icon-arrow-left-circle feather\"></i>Kembali</router-link>\r\n                        <!-- <router-link :to=\"{name: 'soal.create', params:{id:ujian.id_ujian}}\" class=\"btn btn-success me-md-end\" type=\"button\"><i class=\"icon-plus-circle feather\"></i>Tambah</router-link> -->\r\n                        <button class=\"btn btn-primary me-md-end\" data-bs-toggle=\"modal\"\r\n                            data-bs-target=\"#exampleModalCenter\">Tambah</button>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <br>\r\n            <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                    <h4>Detail Ujian</h4>\r\n                    <br>\r\n                    <form class=\"row g-3\">\r\n                        <div class=\"col-md-6\">\r\n                            <label for=\"\" class=\"form-label\">Nama Ujian</label>\r\n                            <input type=\"text\" class=\"form-control\" v-model=\"ujian.nama_ujian\" disabled=\"\">\r\n                            <div v-if=\"Validation.nama_ujian\" class=\"text-danger\">\r\n                                {{Validation.nama_ujian[0]}}\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-md-4\">\r\n                            <label for=\"\" class=\"form-label\">Mata Pelajaran</label>\r\n                            <input type=\"text\" class=\"form-control\" v-model=\"ujian.mapel\" disabled=\"\">\r\n                            <div v-if=\"Validation.mapel\" class=\"text-danger\">\r\n                                {{Validation.mapel[0]}}\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-md-4\">\r\n                            <label for=\"\" class=\"form-label\">Kelas</label>\r\n                            <input type=\"text\" class=\"form-control\" v-model=\"ujian.kelas\" disabled=\"\">\r\n                            <div v-if=\"Validation.kelas_id\" class=\"text-danger\">\r\n                                {{Validation.kelas_id[0]}}\r\n                            </div>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n            <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                    <h4>Soal <span class=\"lead\">{{ujian.nama_ujian}}</span> </h4>\r\n                    <div class=\"mt-4\">\r\n                        <table id=\"data-table\" class=\"table data-table\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>No.</th>\r\n                                    <th>Soal</th>\r\n                                    <th>A</th>\r\n                                    <th>B</th>\r\n                                    <th>C</th>\r\n                                    <th>D</th>\r\n                                    <th>Jawaban</th>\r\n                                    <th>Action</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                <tr v-for=\"(soal, index) in soal.data\" :key=\"index\">\r\n                                    <td>{{++index}}</td>\r\n                                    <td>{{soal.isi_soal}}</td>\r\n                                    <td>{{soal.opsi_a}}</td>\r\n                                    <td>{{soal.opsi_b}}</td>\r\n                                    <td>{{soal.opsi_c}}</td>\r\n                                    <td>{{soal.opsi_d}}</td>\r\n                                    <td>{{soal.opsi_benar}}</td>\r\n                                    <td>\r\n                                        <!-- <router-link :to=\"{name: 'ujian.detail',params:{id:soal.id_soal}}\"\r\n                                            class=\"btn btn-info btn-sm\">Detail</router-link> -->\r\n                                        <button class=\"btn btn-danger btn-sm\"\r\n                                            @click.prevent=\"destroy(soal.id_soal, index)\">Delete</button>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"modal fade\" id=\"exampleModalCenter\" tabindex=\"-1\" aria-labelledby=\"exampleModalCenterLabel\"\r\n            aria-hidden=\"true\">\r\n            <div class=\"modal-dialog modal-dialog-centered\">\r\n                <div class=\"modal-content\">\r\n                    <div class=\"modal-header\">\r\n                        <h5 class=\"modal-title\" id=\"exampleModalCenterLabel\">Tambah Soal</h5>\r\n                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n                    </div>\r\n                    <div class=\"card\">\r\n                        <div class=\"card-body\">\r\n                            <form class=\"row g-3\" @submit.prevent=\"store()\">\r\n                                <div class=\"col-md-12\">\r\n                                    <label for=\"\" class=\"form-label\">Isi Soal</label>\r\n                                    <textarea type=\"text\" class=\"form-control\" v-model=\"addsoal.isi_soal\"></textarea>\r\n                                    <div v-if=\"Validation.isi_soal\" class=\"text-danger\">\r\n                                        {{Validation.isi_soal[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">A</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_a\">\r\n                                    <div v-if=\"Validation.opsi_a\" class=\"text-danger\">\r\n                                        {{Validation.opsi_a[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">B</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_b\">\r\n                                    <div v-if=\"Validation.opsi_b\" class=\"text-danger\">\r\n                                        {{Validation.opsi_b[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">C</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_c\">\r\n                                    <div v-if=\"Validation.opsi_c\" class=\"text-danger\">\r\n                                        {{Validation.opsi_c[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">D</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"addsoal.opsi_d\">\r\n                                    <div v-if=\"Validation.opsi_d\" class=\"text-danger\">\r\n                                        {{Validation.opsi_d[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">Jawaban Benar</label>\r\n                                    <select name=\"soal\" class=\"form-select\" v-model=\"addsoal.opsi_benar\">\r\n                                        <option value=\"opsi_a\">A</option>\r\n                                        <option value=\"opsi_b\">B</option>\r\n                                        <option value=\"opsi_c\">C</option>\r\n                                        <option value=\"opsi_d\">D</option>\r\n                                    </select>\r\n                                    <div v-if=\"Validation.opsi_benar\" class=\"text-danger\">\r\n                                        {{Validation.opsi_benar[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <!-- <div class=\"col-md-6\">\r\n                                                            <label for=\"\" class=\"form-label\">Ujian ID</label>\r\n                                                            <input type=\"text\" class=\"form-control\" v-model=\"addsoal.ujian_id\">\r\n                                                            <div v-if=\"Validation.ujian_id\" class=\"text-danger\">\r\n                                                                {{Validation.ujian_id[0]}}\r\n                                                            </div>\r\n                                                        </div>                                                           -->\r\n                                <div class=\"col-12\">\r\n                                    <button class=\"btn btn-primary\" type=\"submit\">Tambah</button>\r\n                                </div>\r\n                            </form>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- Footer START -->\r\n        <!-- <div class=\"footer fixed-bottom \">\r\n                    <div class=\"footer-content justify-content-md-end\">\r\n                        <p class=\"mb-0\">Copyright © 2022. All rights reserved.</p>\r\n                    </div>\r\n                </div> -->\r\n        <!-- Footer End -->\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive ,ref, onMounted} from 'vue'\r\nimport { useRoute, useRouter} from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    \r\n    setup(){\r\n        let ujian_id = localStorage.ujian_id;\r\n        //data binding\r\n        const ujian = reactive({\r\n            nama_ujian: '',\r\n            id_ujian: '',\r\n            mapel: '',\r\n            kelas: '',\r\n        });\r\n\r\n        const addsoal = reactive({\r\n            isi_soal: '',\r\n            opsi_a: '',\r\n            opsi_b: '',\r\n            opsi_c: '',\r\n            opsi_d: '',\r\n            opsi_benar: '',\r\n            ujian_id: ujian_id,\r\n        });\r\n        \r\n        const Validation = ref([]);\r\n\r\n        const router = useRouter();\r\n        const route = useRoute();\r\n\r\n        onMounted(() => {\r\n            axios.get(`/api/ujian/${route.params.id}`)\r\n            .then((result) => {\r\n                ujian.id_ujian = result.data.data.id_ujian\r\n                ujian.nama_ujian = result.data.data.nama_ujian\r\n                ujian.mapel = result.data.data.mapel.nama_mapel\r\n                ujian.kelas = result.data.data.kelas.nama_kelas\r\n                //set localStorage\r\n                localStorage.setItem(\"ujian_id\", result.data.data.id_ujian)\r\n\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        let soal = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get mapel from api\r\n            axios.get(`/api/getSoalByUjian/${route.params.id}`)\r\n            .then((result) => {\r\n                soal.value = result.data\r\n            }).catch((err) => {\r\n                console.log(err.response)\r\n            });\r\n        });\r\n\r\n\r\n        function store() {\r\n            console.log(addsoal)\r\n            axios.post(\r\n                '/api/soal',\r\n                addsoal\r\n            )\r\n             .then(() => {\r\n                localStorage.removeItem('ujian_id');\r\n                router.push({\r\n                    name : 'ujian.index'\r\n                })\r\n                \r\n            }).catch((err) => {\r\n                Validation.value = err.response.data\r\n            });\r\n        }\r\n\r\n\r\n        function destroy(id,index){\r\n            axios.delete(\r\n                `/api/soal/${id}`\r\n            )\r\n            .then(() => {\r\n                soal.value.data.splice(index,1)                \r\n                router.push({\r\n                    name: 'ujian.index'\r\n                })\r\n            }).catch((err) => {\r\n                console.log(err.response.data);\r\n            });\r\n        } \r\n\r\n        return {\r\n            ujian,\r\n            soal,\r\n            store,destroy,\r\n            addsoal,\r\n            Validation,\r\n            router,\r\n        }\r\n    }    \r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}