\">Nama Lengkap</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"siswa.nama_siswa\">\r\n                                    <div v-if=\"Validation.nama_siswa\" class=\"text-danger\">\r\n                                        {{Validation.nama_siswa[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-4\">\r\n                                    <label for=\"\" class=\"form-label\">NIS</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"siswa.nis_siswa\">\r\n                                    <div v-if=\"Validation.nis_siswa\" class=\"text-danger\">\r\n                                        {{Validation.nis_siswa[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-4\">\r\n                                    <label for=\"\" class=\"form-label\">Kelas</label>\r\n                                    <select name=\"kelas\" class=\"form-select\" v-model=\"siswa.kelas_id\">\r\n                                        <option v-for=\"(kelas, index) in kelas.data\" :key=\"index\" :value=\"kelas.id_kelas\">{{kelas.nama_kelas}}</option>\r\n                                    </select>\r\n                                    <div v-if=\"Validation.kelas_id\" class=\"text-danger\">\r\n                                        {{Validation.kelas_id[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">Alamat</label>\r\n                                    <textarea type=\"text\" class=\"form-control\" v-model=\"siswa.alamat_siswa\"></textarea>\r\n                                    <div v-if=\"Validation.alamat_siswa\" class=\"text-danger\">\r\n                                        {{Validation.alamat_siswa[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-12\">\r\n                                    <button class=\"btn btn-warning\" type=\"submit\">Update</button>\r\n                                </div>\r\n                            </form>  \r\n                        </div>\r\n                    </div> -->\r\n                     <div class=\"card\">\r\n                        <div class=\"card-body\">\r\n                            <h4>Data Siswa</h4>\r\n                            <div class=\"mt-4\">\r\n                                <table id=\"data-table\" class=\"table data-table\">\r\n                                    <thead>\r\n                                        <tr>\r\n                                            <th>Nama</th>\r\n                                            <th>NIS</th>\r\n                                            <th>Kelas</th>\r\n                                            <th>Alamat</th>\r\n                                            <th>Action</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody>\r\n                                        <tr v-for=\"(siswa, index) in siswa.data\" :key=\"index\">\r\n                                            <td>{{siswa.nama_siswa}}</td>\r\n                                            <td>{{siswa.nis_siswa}}</td>\r\n                                            <td>{{siswa.kelas.nama_kelas}}</td>\r\n                                            <td>{{siswa.alamat_siswa}}</td>\r\n                                            <td>\r\n                                                <router-link :to=\"{name: 'siswa.detail',params:{id:siswa.id_siswa}}\" class=\"btn btn-info btn-sm\">Detail</router-link>\r\n                                                <router-link :to=\"{name: 'nilai.detail',params:{id:siswa.id_siswa}}\" class=\"btn btn-warning btn-sm\">Nilai</router-link>\r\n                                                <button class=\"btn btn-danger btn-sm\" @click.prevent=\"destroy(siswa.id_siswa, index)\">Delete</button>\r\n                                            </td>\r\n                                        </tr>                                                                                                 \r\n                                    </tbody>                                   \r\n                                </table>\r\n                            </div>\r\n                        </div>\r\n                    </div>                    \r\n                </div>\r\n                <!-- Footer START -->\r\n                <div class=\"footer fixed-bottom \">\r\n                    <div class=\"footer-content justify-content-md-end\">\r\n                        <p class=\"mb-0\">Copyright © 2022. All rights reserved.</p>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer End -->\r\n            </div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive ,ref, onMounted} from 'vue'\r\nimport { useRoute, useRouter} from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    setup(){\r\n        //data binding\r\n        const siswa = reactive({\r\n            nama_siswa: '',\r\n            nis_siswa: '',\r\n            kelas_id: '',\r\n            alamat_siswa: '',\r\n        });\r\n\r\n        const Validation = ref([]);\r\n\r\n        const router = useRouter();\r\n        const route = useRoute();\r\n\r\n        onMounted(() => {\r\n            axios.get(`http://localhost:8000/api/siswa/${route.params.id}`)\r\n            .then((result) => {\r\n                siswa.nama_siswa = result.data.data.nama_siswa,\r\n                siswa.nis_siswa = result.data.data.nis_siswa,\r\n                siswa.kelas_id = result.data.data.kelas_id,\r\n                siswa.alamat_siswa = result.data.data.alamat_siswa\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        let kelas = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get kelas from api\r\n            axios.get('http://localhost:8000/api/kelas')\r\n            .then((result) => {\r\n                kelas.value = result.data\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        let data_siswa = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get data_siswa from api\r\n            // axios.get(`http://localhost:8000/api/data-siswa/${route.params.id}`)\r\n            axios.get('http://localhost:8000/api/data-siswa')\r\n            .then((result) => {\r\n                data_siswa.value = result.data\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        function update() {\r\n            axios.put(\r\n                `http://localhost:8000/api/siswa/${route.params.id}`,\r\n                siswa\r\n            )\r\n             .then(() => {\r\n                router.push({\r\n                    name : 'siswa.index'\r\n                })\r\n            }).catch((err) => {\r\n                Validation.value = err.response.data\r\n            });\r\n        }\r\n\r\n        return {\r\n            siswa,\r\n            data_siswa,\r\n            kelas,\r\n            Validation,\r\n            router,\r\n            update\r\n        }\r\n    }    \r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}