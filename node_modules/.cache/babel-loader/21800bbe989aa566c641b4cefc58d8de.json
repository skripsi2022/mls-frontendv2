{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  // \n  setup() {\n    let guru = ref([]);\n    onMounted(() => {\n      //get guru from api\n      axios.get('http://localhost:8000/api/guru').then(result => {\n        guru.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`http://localhost:8000/api/guru/${id}`).then(() => {\n        guru.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      guru,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACX;AACAC,OAAK,GAAE;AACH,QAAIC,IAAG,GAAIF,GAAG,CAAC,EAAD,CAAd;AAEAD,aAAS,CAAC,MAAM;AACZ;AACAD,WAAK,CAACK,GAAN,CAAU,gCAAV,EACCC,IADD,CACOC,MAAD,IAAY;AACdH,YAAI,CAACI,KAAL,GAAaD,MAAM,CAACE,IAApB;AACH,OAHD,EAGGC,KAHH,CAGUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAhB;AACH,OALD;AAMH,KARQ,CAAT;;AAUC,aAASC,OAAT,CAAiBC,EAAjB,EAAoBC,KAApB,EAA0B;AACvBjB,WAAK,CAACkB,MAAN,CACK,kCAAiCF,EAAG,EADzC,EAGCV,IAHD,CAGM,MAAM;AACRF,YAAI,CAACI,KAAL,CAAWC,IAAX,CAAgBU,MAAhB,CAAuBF,KAAvB,EAA6B,CAA7B;AACH,OALD,EAKGP,KALH,CAKUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OAPD;AAQJ;;AAEA,WAAO;AACHL,UADG;AACEW;AADF,KAAP;AAGJ;;AA7BW,CAAf","names":["axios","onMounted","ref","setup","guru","get","then","result","value","data","catch","err","console","log","response","destroy","id","index","delete","splice"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Guru\\IndexView.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n                <div class=\"main\">                    \r\n                    <div class=\"page-header\">\r\n                        <h4 class=\"page-title\">Data Guru</h4>\r\n