{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const jurusan = reactive({\n      nama_jurusan: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/jurusan/${route.params.id}`).then(result => {\n        jurusan.nama_jurusan = result.data.data.nama_jurusan;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n\n    function update() {\n      axios.put(`http://localhost:8000/api/jurusan/${route.params.id}`, jurusan).then(() => {\n        router.push({\n          name: 'jurusan.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      jurusan,\n      Validation,\n      router,\n      update\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwDA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,OAAM,GAAIP,QAAQ,CAAC;AACrBQ,kBAAY,EAAE;AADO,KAAD,CAAxB;AAIA,UAAMC,UAAS,GAAIR,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMS,MAAK,GAAIN,SAAS,EAAxB;AACA,UAAMO,KAAI,GAAIR,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACO,GAAN,CAAW,qCAAoCD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA/D,EACCC,IADD,CACOC,MAAD,IAAY;AACdT,eAAO,CAACC,YAAR,GAAuBQ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBT,YAAxC;AACH,OAHD,EAGGU,KAHH,CAGUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OALD;AAMH,KAPQ,CAAT;;AASA,aAASM,MAAT,GAAkB;AACdlB,WAAK,CAACmB,GAAN,CACK,qCAAoCb,KAAK,CAACE,MAAN,CAAaC,EAAG,EADzD,EAEIP,OAFJ,EAIEQ,IAJF,CAIO,MAAM;AACTL,cAAM,CAACe,IAAP,CAAY;AACRC,cAAG,EAAI;AADC,SAAZ;AAGH,OARD,EAQGR,KARH,CAQUC,GAAD,IAAS;AACdV,kBAAU,CAACkB,KAAX,GAAmBR,GAAG,CAACG,QAAJ,CAAaL,IAAhC;AACH,OAVD;AAWJ;;AAEA,WAAO;AACHV,aADG;AAEHE,gBAFG;AAGHC,YAHG