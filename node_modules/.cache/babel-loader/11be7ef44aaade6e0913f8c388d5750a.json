.get(`http://localhost:8000/api/jurusan/${route.params.id}`)\r\n            .then((result) => {\r\n                jurusan.nama_jurusan = result.data.data.nama_jurusan\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        function update() {\r\n            axios.put(\r\n                `http://localhost:8000/api/jurusan/${route.params.id}`,\r\n                jurusan\r\n            )\r\n             .then(() => {\r\n                router.push({\r\n                    name : 'jurusan.index'\r\n                })\r\n            }).catch((err) => {\r\n                Validation.value = err.response.data\r\n            });\r\n        }\r\n\r\n        return {\r\n            jurusan,\r\n            Validation,\r\n            router,\r\n            update\r\n        }\r\n    }    \r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}