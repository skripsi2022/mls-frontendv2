    </div> -->\r\n                        <div class=\"col-lg-9\">\r\n                            <div class=\"\">                            \r\n                                <div class=\"card-body\" style=\"position: relative;\">\r\n                                     <div class=\"d-grid gap-2 d-md-flex justify-content-md-end\">\r\n                                        <router-link :to=\"{name: 'guru.mapel.create'}\" class=\"btn btn-primary me-md-end\" type=\"button\"><i class=\"icon-plus-circle feather\"></i>Tambah</router-link>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                        \r\n                    </div>\r\n                    <div class=\"card\">\r\n                        <div class=\"card-body\">\r\n                            <h4>Data Mata Pelajaran</h4>\r\n                            <div class=\"mt-4\">\r\n                                <table id=\"data-table\" class=\"table data-table\">\r\n                                    <thead>\r\n                                        <tr>\r\n                                            <th>Mata Pelajaran</th>                                           \r\n                                            <th>Action</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody>\r\n                                        <tr v-for=\"(mapel, index) in mapel.data.data\" :key=\"index\"> \r\n                                            <td>{{mapel.nama_mapel}}</td>\r\n                                            <td>\r\n                                                <router-link :to=\"{name: 'mapel.detail',params:{id:mapel.id_mapel}}\" class=\"btn btn-info btn-sm\">Detail</router-link>\r\n                                                <button class=\"btn btn-danger btn-sm\" @click.prevent=\"destroy(mapel.id_mapel, index)\">Delete</button>\r\n                                            </td>\r\n                                        </tr>                                                                                                 \r\n                                    </tbody>                                   \r\n                                </table>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer START -->\r\n                <div class=\"footer fixed-bottom \">\r\n                    <div class=\"footer-content justify-content-md-end\">\r\n                        <p class=\"mb-0\">Copyright Â© 2022. All rights reserved.</p>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer End -->\r\n            </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\nimport {onMounted, reactive} from 'vue'\r\n\r\nexport default {\r\n    // \r\n     data() {\r\n        return {\r\n            //state loggedIn with localStorage\r\n            loggedIn: localStorage.getItem('loggedIn'),\r\n            //state token\r\n            token: localStorage.getItem('token'),\r\n        }\r\n    },\r\n\r\n    setup(){\r\n        // let mapel = ref([]);\r\n        const mapel = reactive({\r\n            nama_mapel: '',\r\n            id_mapel: '',\r\n            guru_id:''\r\n        });\r\n        onMounted(() => {\r\n            //get mapel by guru\r\n            axios.get('/api/mapelGuru',{\r\n                params: {\r\n                    id: localStorage.getItem('id')\r\n                }\r\n            })\r\n            .then((result) => {\r\n                // mapel.value = result.data\r\n                mapel.id_mapel = result.data.data.id_mapel\r\n                mapel.nama_mapel = result.data.data.nama_mapel\r\n                mapel.guru_id = result.data.data.guru_id\r\n                //set localStorage\r\n                localStorage.setItem(\"guru_id\", result.data.data.guru_id)\r\n                console.log(result)\r\n            }).catch((err) => {\r\n                console.log(err.response)\r\n            });\r\n        });\r\n\r\n        function destroy(id,index){\r\n            axios.delete(\r\n                `/api/mapel/${id}`\r\n            )\r\n            .then(() => {\r\n                mapel.value.data.splice(index,1)\r\n            }).catch((err) => {\r\n                console.log(err.response.data);\r\n            });\r\n        } \r\n\r\n        return {\r\n            mapel,destroy,\r\n        }\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}