{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, reactive } from 'vue';\nexport default {\n  // \n  data() {\n    return {\n      //state loggedIn with localStorage\n      loggedIn: localStorage.getItem('loggedIn'),\n      //state token\n      token: localStorage.getItem('token')\n    };\n  },\n\n  setup() {\n    // let mapel = ref([]);\n    const mapel = reactive({\n      nama_mapel: '',\n      id_mapel: '',\n      guru_id: ''\n    });\n    onMounted(() => {\n      //get mapel by guru\n      axios.get('/api/mapelGuru', {\n        params: {\n          id: localStorage.getItem('id')\n        }\n      }).then(result => {\n        // mapel.value = result.data\n        mapel.id_mapel = result.data.data.id_mapel;\n        mapel.nama_mapel = result.data.data.nama_mapel;\n        mapel.guru_id = result.data.data.guru_id; //set localStorage\n\n        localStorage.setItem(\"guru_id\", result.data.data.guru_id);\n        console.log(result);\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`/api/mapel/${id}`).then(() => {\n        mapel.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      mapel,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA8FA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,QAAnB,QAAkC,KAAlC;AAEA,eAAe;AACX;AACCC,MAAI,GAAG;AACJ,WAAO;AACH;AACAC,cAAQ,EAAEC,YAAY,CAACC,OAAb,CAAqB,UAArB,CAFP;AAGH;AACAC,WAAK,EAAEF,YAAY,CAACC,OAAb,CAAqB,OAArB;AAJJ,KAAP;AAMH,GATU;;AAWXE,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIP,QAAQ,CAAC;AACnBQ,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,aAAO,EAAC;AAHW,KAAD,CAAtB;AAKAX,aAAS,CAAC,MAAM;AACZ;AACAD,WAAK,CAACa,GAAN,CAAU,gBAAV,EAA2B;AACvBC,cAAM,EAAE;AACJC,YAAE,EAAEV,YAAY,CAACC,OAAb,CAAqB,IAArB;AADA;AADe,OAA3B,EAKCU,IALD,CAKOC,MAAD,IAAY;AACd;AACAR,aAAK,CAACE,QAAN,GAAiBM,MAAM,CAACd,IAAP,CAAYA,IAAZ,CAAiBQ,QAAlC;AACAF,aAAK,CAACC,UAAN,GAAmB