{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const nilai = reactive({\n      nama_siswa: '',\n      nama_ujian: '',\n      nilai: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`/api/getNilai/${route.params.id}`).then(result => {\n        nilai.nama_siswa = result.data.data.siswa.nama_siswa, nilai.nama_ujian = result.data.data.ujian.nama_ujian, nilai.nilai = result.data.data.nilai;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n\n    function update() {\n      console.log(nilai);\n      axios.put(`/api/updNilai/${route.params.id}`, nilai).then(() => {\n        router.push({\n          name: 'guru.nilai.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      nilai,\n      Validation,\n      router,\n      update\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoEA,SAASA,QAAT,EAAmBC,GAAnB,EAAwBC,SAAxB,QAAyC,KAAzC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAoC,YAApC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAG;AACJ;AACA,UAAMC,KAAI,GAAIP,QAAQ,CAAC;AACnBQ,gBAAU,EAAE,EADO;AAEnBC,gBAAU,EAAE,EAFO;AAGnBF,WAAK,EAAE;AAHY,KAAD,CAAtB;AAMA,UAAMG,UAAS,GAAIT,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMU,MAAK,GAAIP,SAAS,EAAxB;AACA,UAAMQ,KAAI,GAAIT,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACQ,GAAN,CAAW,iBAAgBD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA3C,EACKC,IADL,CACWC,MAAD,IAAY;AACdV,aAAK,CAACC,UAAN,GAAmBS,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBC,KAAjB,CAAuBX,UAA1C,EACID,KAAK,CAACE,UAAN,GAAmBQ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBE,KAAjB,CAAuBX,UAD9C,EAEIF,KAAK,CAACA,KAAN,GAAcU,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBX,KAFnC;AAGH,OALL,EAKOc,KALP,CAKcC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaP,IAAzB;AACH,OAPL;AAQH,KATQ,CAAT;;AAWA,aAAS