{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const ujian = reactive({\n      nama_mapel: '',\n      guru_id: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n\n    function store() {\n      axios.post('http://localhost:8000/api/ujian', ujian).then(() => {\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    let guru = ref([]);\n    onMounted(() => {\n      //get siswa from api\n      axios.get('http://localhost:8000/api/guru').then(result => {\n        guru.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n    return {\n      ujian,\n      guru,\n      Validation,\n      router,\n      store\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6DA,SAAQA,QAAR,EAAkBC,GAAlB,EAAsBC,SAAtB,QAAsC,KAAtC;AACA,SAASC,SAAT,QAAyB,YAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIN,QAAQ,CAAC;AACnBO,gBAAU,EAAE,EADO;AAEnBC,aAAO,EAAE;AAFU,KAAD,CAAtB;AAKA,UAAMC,UAAS,GAAIR,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMS,MAAK,GAAIP,SAAS,EAAxB;;AAEA,aAASQ,KAAT,GAAiB;AACbP,WAAK,CAACQ,IAAN,CACI,iCADJ,EAEIN,KAFJ,EAIEO,IAJF,CAIO,MAAM;AACTH,cAAM,CAACI,IAAP,CAAY;AACRC,cAAG,EAAI;AADC,SAAZ;AAGH,OARD,EAQGC,KARH,CAQUC,GAAD,IAAS;AACdR,kBAAU,CAACS,KAAX,GAAmBD,GAAG,CAACE,QAAJ,CAAaC,IAAhC;AACH,OAVD;AAWJ;;AAEA,QAAIC,IAAG,GAAIpB,GAAG,CAAC,EAAD,CAAd;AAEAC,aAAS,CAAC,MAAM;AACZ;AACAE,WAAK,CAACkB,GAAN,CAAU,gCAAV,EACCT,IADD,CACOU,MAAD,IAAY;AACdF,YAAI,CAACH,KAAL,GAAaK,MAAM,CAACH,IAApB;AACH,OAHD,EAGGJ,KAHH,CAGUC,GAAD,IAAS;AACdO,eAAO,CAACC,GAAR,CAAYR,GAAG,CAACE,QAAhB;AACH,OALD;AAMH,KARQ,CAAT;AAUA,WAAO;AACHb,WADG;AAEHe,UAFG;AAGHZ,gBAHG;AAIHC,YAJG;AAKHC;AALG,KAAP;AAOJ;;AA7CW,CAAf","names":["reactive","ref","onMounted","useRouter","axios","setup","ujian","nama