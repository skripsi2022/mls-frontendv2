{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const nilai = reactive({\n      nama_siswa: '',\n      nama_ujian: '',\n      nilai: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`/api/getNilai/${route.params.id}`).then(result => {\n        nilai.nama_siswa = result.data.data.nama_siswa, nilai.nis_siswa = result.data.data.nis_siswa, nilai.kelas_id = result.data.data.kelas_id, nilai.alamat_siswa = result.data.data.alamat_siswa;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let kelas = ref([]);\n    onMounted(() => {\n      //get kelas from api\n      axios.get('/api/kelas').then(result => {\n        kelas.value = result.data;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n\n    function update() {\n      axios.put(`/api/siswa/${route.params.id}`, siswa).then(() => {\n        router.push({\n          name: 'siswa.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      siswa,\n      kelas,\n      Validation,\n      router,\n      update\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmEA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIP,QAAQ,CAAC;AACnBQ,gBAAU,EAAE,EADO;AAEnBC,gBAAU,EAAE,EAFO;AAGnBF,WAAK,EAAE;AAHY,KAAD,CAAtB;AAMA,UAAMG,UAAS,GAAIT,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMU,MAAK,GAAIP,SAAS,EAAxB;AACA,UAAMQ,KAAI,GAAIT,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACQ,GAAN,CAAW,iBAAgBD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA3C,EACCC,IADD,CACOC,MAAD,IAAY;AACdV,aAAK,CAACC,UAAN,GAAmBS,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBV,UAApC,EACAD,KAAK,CAACY,SAAN