{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  methods: {\n    showAlert() {\n      // Use sweetalert2\n      this.$swal({\n        title: 'Do you want to save the changes?',\n        showDenyButton: true,\n        showCancelButton: true,\n        confirmButtonText: 'Save',\n        denyButtonText: `Don't save`\n      }).then(result => {\n        if (result.isConfirmed) {\n          this.$swal('Saved!', '', 'success');\n        } else if (result.isDenied) {\n          this.$swal('Changes are not saved', '', 'info');\n        }\n      });\n    }\n\n  },\n\n  data() {\n    return {\n      //state loggedIn with localStorage\n      loggedIn: localStorage.getItem('loggedIn'),\n      //state token\n      token: localStorage.getItem('token')\n    };\n  },\n\n  setup() {\n    let mapel = ref([]);\n    onMounted(() => {\n      //get mapel by guru\n      axios.get('/api/mapelGuru', {\n        params: {\n          id: localStorage.getItem('id')\n        }\n      }).then(result => {\n        mapel.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id) {\n      axios.delete(`/api/mapel/${id}`).then(() => {\n        // mapel.value.splice(mapel.value.indexOf(id),1)\n        window.location.reload();\n      }).catch(err => {\n        console.log(err.response);\n      });\n    }\n\n    return {\n      mapel,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAGA,eAAe;AACVC,SAAO,EAAE;AACNC,aAAS,GAAG;AACR;AACA,WAAKC,KAAL,CAAW;AACPC,aAAK,EAAE,kCADA;AAEPC,sBAAc,EAAE,IAFT;AAGPC,wBAAgB,EAAE,IAHX;AAIPC,yBAAiB,EAAE,MAJZ;AAKPC,sBAAc,EAAG;AALV,OAAX,EAMGC,IANH,CAMSC,MAAD,IAAY;AAChB,YAAIA,MAAM,CAACC,WAAX,EAAwB;AACpB,eAAKR,KAAL,CAAW,QAAX,EAAqB,EAArB,EAAyB,SAAzB;AACJ,SAFA,MAEO,IAAIO,MAAM,CAACE,QAAX,EAAqB;AACxB,eAAKT,KAAL,CAAW,uBAAX,EAAoC,EAApC,EAAwC,MAAxC;AACJ;AACH,OAZD;AAaH;;AAh