{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nimport Swal from 'sweetalert2';\nexport default {\n  methods: {\n    destroy1(id) {\n      axios.delete(`/api/ujian/${id}`).then(() => {\n        //  ujian.value.data.splice(index, 1)           \n        this.$swal({\n          title: 'Success!',\n          text: 'Data Ujian Berhasil dihapus !',\n          icon: 'success',\n          confirmButtonText: 'Lanjut !'\n        });\n      }).catch(err => {\n        console.log(err.response.data);\n        this.$swal({\n          title: 'error!',\n          text: 'Kosongkan Soal terlebih dahulu !',\n          icon: 'error',\n          confirmButtonText: 'Lanjut !'\n        });\n      });\n    }\n\n  },\n\n  setup() {\n    let ujian = ref([]);\n    onMounted(() => {\n      //get ujian from api\n      console.log(\"ujian id : \" + localStorage.ujian_id);\n      axios.get('/api/ujian').then(result => {\n        ujian.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    async function destroy(id, index) {\n      Swal.fire({\n        title: 'Hapus Data Ujian',\n        text: \"Yakin dihapus ?\",\n        icon: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#3085d6',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Hapus !'\n      }).then(result => {\n        if (result.isConfirmed) {\n          axios.delete(`/api/ujian/${id}`).then(() => {\n            ujian.value.data.splice(index, 1);\n            Swal.fire({\n              title: 'success!',\n              text: 'Ujian Be',\n              icon: 'error',\n              confirmButtonText: 'Lanjut !'\n            });\n            window.location.reload();\n          }).catch(err => {\n            console.log(err.response.data);\n            Swal.fire({\n              title: 'error!',\n              text: 'Kosongkan Soal terlebih dahulu !',\n              icon: 'error',\n              confirmButtonText: 'Lanjut !'\n            });\n          });\n        }\n      });\n    }\n\n    return {\n      ujian,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AACA,OAAOC,IAAP,MAAiB,aAAjB;AAEA,eAAe;AAEVC,SAAM,EAAI;AACPC,YAAQ,CAACC,EAAD,EAAI;AAEPN,WAAK,CAACO,MAAN,CACK,cAAaD,EAAG,EADrB,EAGKE,IAHL,CAGU,MAAM;AACT;AACC,aAAKC,KAAL,CAAW;AACRC,eAAK,EAAE,UADC;AAERC,cAAI,EAAE,+BAFE;AAGRC,cAAI,EAAE,SAHE;AAIRC,2BAAiB,EAAE;AAJX,SAAX;AAMH,OAXL,EAWOC,KAXP,CAWcC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaC,IAAzB;AACA,aAAKV,KAAL,CAAW;AACRC,eAAK,EAAE,QADC;AAERC,cAAI,EAAE,kCAFE;AAGRC,cAAI,EAAE,OAHE;AAIRC,2BAAiB,EAAE;AAJX,SAAX;AAMH,OAnBL;AAoBJ;;AAvBM,GAFA;;AA2BXO,OAAK,GAAE;AACH,QAAIC,KAAI,GAAInB,GAAG,CAAC,EAAD,CAAf;AAEAD,aAAS,CAAC,MAAM;AACZ;AACGe,aAAO,CAACC,GAAR,CAAY,gBAAgBK,YAAY,CAACC,QAAzC;AACHvB,WAAK,CAACwB,GAAN,CAAU,YAAV,EACChB,IADD,CACOiB,MAAD,IAAY;AACdJ,aAAK,CAACK,KAAN,GAAcD,MAAM,CAACN,IAArB;AAEH,OAJD,EAIGL,KAJH,CAIUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAhB;AACH,OAND;AAOH,KAVQ,CAAT;;AAYA,mBAAeS,OAAf,CAAuBrB,EAAvB,EAA0BsB,KAA1B,EAAgC;AAC5BzB,UAAI,CAAC0B,IAAL,CAAU;AACNnB,aAAK,EAAE,kBADD;AAENC,YAAI,EAAE,iBAFA;AAGNC,YAAI,EAAE,SAHA;AAINkB,wBAAgB,EAAE,IAJZ;AAKNC,0BAAkB,EAAE,SALd;AAMNC,yBAAiB,EAAE,MANb;AAONnB,yBAAiB,EAAE;AAPb,OAAV,EAQGL,IARH,CAQSiB,MAAD,IAAY;AAChB,YAAIA,MAAM,CAACQ,WAAX,EAAwB;AACpBjC,eAAK,CAACO,MAAN,CACK,cAAaD,EAAG,EADrB,EAGKE,IAHL,CAGU,MAAM;AACRa,iBAAK,CAACK,KAAN,CAAYP,IAAZ,CAAiBe,MAAjB,CAAwBN,KAAxB,EAA+B,CAA/B;AACAzB,gBAAI,CAAC0B,IAAL,CAAU;AACNnB,mBAAK,EAAE,UADD;AAENC,kBAAI,EAAE,UAFA;AAGNC,kBAAI,EAAE,OAHA;AAINC,+BAAiB,EAAE;AAJb,aAAV;AAOAsB,kBAAM,CAACC,QAAP,CAAgBC,MAAhB;AACH,WAbL,EAaOvB,KAbP,CAacC,GAAD,IAAS;AACdC,mBAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaC,IAAzB;AACAhB,gBAAI,CAAC0B,IAAL,CAAU;AACNnB,mBAAK,EAAE,QADD;AAENC,kBAAI,EAAE,kCAFA;AAGNC,kBAAI,EAAE,OAHA;AAINC,+BAAiB,EAAE;AAJb,aAAV;AAOH,WAtBL;AAwBJ;AACH,OAnCD;AAoCJ;;AAEA,WAAO;AACHQ,WADG;AACGM;AADH,KAA