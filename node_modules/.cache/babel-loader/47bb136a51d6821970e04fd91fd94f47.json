{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    let ujian_id = localStorage.ujian_id; //data binding\n\n    const ujian = reactive({\n      nama_ujian: '',\n      id_ujian: '',\n      mapel_id: '',\n      kelas_id: ''\n    });\n    const addsoal = reactive({\n      isi_soal: '',\n      opsi_a: '',\n      opsi_b: '',\n      opsi_c: '',\n      opsi_d: '',\n      opsi_benar: '',\n      ujian_id: ujian_id\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`/api/ujian/${route.params.id}`).then(result => {\n        ujian.id_ujian = result.data.data.id_ujian;\n        ujian.nama_ujian = result.data.data.nama_ujian;\n        ujian.mapel_id = result.data.data.mapel_id;\n        ujian.kelas_id = result.data.data.kelas_id; //set localStorage\n\n        localStorage.setItem(\"ujian_id\", result.data.data.id_ujian);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let mapel = ref([]);\n    onMounted(() => {\n      //get mapel from api\n      axios.get('/api/mapel').then(result => {\n        mapel.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n    let soal = ref([]);\n    onMounted(() => {\n      //get mapel from api\n      axios.get(`/api/getSoalByUjian/${route.params.id}`).then(result => {\n        soal.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n    let kelas = ref([]);\n    onMounted(() => {\n      //get kelas from api\n      axios.get('/api/kelas').then(result => {\n        kelas.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function store() {\n      console.log(addsoal);\n      axios.post('/api/soal', addsoal).then(() => {\n        window.location.reload();\n        localStorage.removeItem('ujian_id');\n        router.push({\n          name: 'guru.ujian.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    function update() {\n      axios.put(`/api/ujian/${route.params.id}`, ujian).then(() => {\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    function destroy(id, index) {\n      axios.delete(`/api/soal/${id}`).then(() => {\n        soal.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      ujian,\n      mapel,\n      kelas,\n      soal,\n      store,\n      destroy,\n      update,\n      addsoal,\n      Validation,\n      router\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkLA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AAEXC,OAAK,GAAE;AACH,QAAIC,QAAO,GAAIC,YAAY,CAACD,QAA5B,CADG,CAEH;;AACA,UAAME,KAAI,GAAIT,QAAQ,CAAC;AACnBU,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,cAAQ,EAAE,EAHS;AAInBC,cAAQ,EAAE;AAJS,KAAD,CAAtB;AAOA,UAAMC,OAAM,GAAId,QAAQ,CAAC;AACrBe,cAAQ,EAAE,EADW;AAErBC,YAAM,EAAE,EAFa;AAGrBC,YAAM,EAAE,EAHa;AAIrBC,YAAM,EAAE,EAJa;AAKrBC,YAAM,EAAE,EALa;AAMrBC,gBAAU,EAAE,EANS;AAOrBb,cAAQ,EAAEA;AAPW,KAAD,CAAxB;AAUA,UAAMc,UAAS,GAAIpB,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMqB,MAAK,GAAIlB,SAAS,EAAxB;AACA,UAAMmB,KAAI,GAAIpB,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACmB,GAAN,CAAW,cAAaD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAAxC,EACCC,IADD,CACOC,MAAD,IAAY;AACdnB,aAAK,CAACE,QAAN,GAAiBiB,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBlB,QAAlC;AACAF,aAAK,CAACC,UAAN,GAAmBkB,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBnB,UAApC;AACAD,aAAK,CAACG,QAAN,GAAiBgB,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBjB,QAAlC;AACAH,aAAK,CAACI,QAAN,GAAiBe,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBhB,QAAlC,CAJc,CAKd;;AACAL,oBAAY,CAACsB,OAAb,C