ta.data.mapel.nama_mapel\r\n                ujian.kelas = result.data.data.kelas.nama_kelas\r\n                \r\n                //set localStorage\r\n                localStorage.setItem(\"ujian_id\", result.data.data.id_ujian)\r\n                console.log(result)\r\n\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        let soal = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get mapel from api\r\n            axios.get(`/api/getSoalByUjian/${route.params.id}`)\r\n            .then((result) => {\r\n                soal.value = result.data\r\n            }).catch((err) => {\r\n                console.log(err.response)\r\n            });\r\n        });\r\n\r\n    \r\n        function back(){\r\n            console.log(\"ashiap\")\r\n            // localStorage.removeItem('ujian_id');\r\n           \r\n            // router.push({\r\n            //     name : 'ujian.index'\r\n            // })\r\n        }\r\n\r\n        function store() {\r\n            console.log(addsoal)\r\n            router.go({\r\n                name: 'soal.index',\r\n                params: { id: ujian_id }\r\n            })\r\n            // axios.post(\r\n            //     '/api/soal',\r\n            //     addsoal\r\n            // )\r\n            //  .then(() => {\r\n            //     localStorage.removeItem('ujian_id');\r\n            //     router.push({\r\n            //         name : 'ujian.index'\r\n            //     })\r\n                \r\n            // }).catch((err) => {\r\n            //     Validation.value = err.response.data\r\n            // });\r\n        }\r\n\r\n\r\n        function destroy(id,index){\r\n            axios.delete(\r\n                `/api/soal/${id}`\r\n            )\r\n            .then(() => {\r\n                soal.value.data.splice(index,1)                \r\n                router.push({\r\n                    name: 'ujian.index'\r\n                })\r\n            }).catch((err) => {\r\n                console.log(err.response.data);\r\n            });\r\n        } \r\n\r\n        return {\r\n            ujian,\r\n            soal,\r\n            back,\r\n            store,destroy,\r\n            addsoal,\r\n            Validation,\r\n            router,\r\n        }\r\n    }    \r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}