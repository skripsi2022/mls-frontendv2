{"ast":null,"code":"import { reactive, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const soal = reactive({\n      isi_soal: '',\n      opsi_a: '',\n      opsi_b: '',\n      opsi_c: '',\n      opsi_d: '',\n      opsi_benar: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n\n    function store() {\n      axios.post('http://localhost:8000/api/soal', soal).then(() => {\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      soal,\n      Validation,\n      router,\n      store\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsEA,SAAQA,QAAR,EAAkBC,GAAlB,QAA4B,KAA5B;AACA,SAASC,SAAT,QAAyB,YAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,IAAG,GAAIL,QAAQ,CAAC;AAClBM,cAAQ,EAAE,EADQ;AAElBC,YAAM,EAAE,EAFU;AAGlBC,YAAM,EAAE,EAHU;AAIlBC,YAAM,EAAE,EAJU;AAKlBC,YAAM,EAAE,EALU;AAMlBC,gBAAU,EAAE;AANM,KAAD,CAArB;AASA,UAAMC,UAAS,GAAIX,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMY,MAAK,GAAIX,SAAS,EAAxB;;AAEA,aAASY,KAAT,GAAiB;AACbX,WAAK,CAACY,IAAN,CACI,gCADJ,EAEIV,IAFJ,EAIEW,IAJF,CAIO,MAAM;AACTH,cAAM,CAACI,IAAP,CAAY;AACRC,cAAG,EAAI;AADC,SAAZ;AAGH,OARD,EAQGC,KARH,CAQUC,GAAD,IAAS;AACdR,kBAAU,CAACS,KAAX,GAAmBD,GAAG,CAACE,QAAJ,CAAaC,IAAhC;AACH,OAVD;AAWJ;;AAEA,WAAO;AACHlB,UADG;AAEHO,gBAFG;AAGHC,YAHG;AAIHC;AAJG,KAAP;AAMJ;;AApCW,CAAf","names":["reactive","ref","useRouter","axios","setup","soal","isi_soal","opsi_a","opsi_b","opsi_c","opsi_d","opsi_benar","Validation","router","store","post","then","push","name","catch","err","value","response","data"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Soal\\CreateView.vue"],"sourcesContent":["<template>\r\n     <div class=\"content container\">\r\n                <div class=\"main\">\r\n                    <div class=\"