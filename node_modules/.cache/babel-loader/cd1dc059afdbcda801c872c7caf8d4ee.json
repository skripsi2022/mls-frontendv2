{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  // \n  setup() {\n    let mapel = ref([]);\n    onMounted(() => {\n      //get mapel from api\n      axios.get('http://localhost:8000/api/mapel').then(result => {\n        mapel.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`http://localhost:8000/api/mapel/${id}`).then(() => {\n        mapel.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      mapel,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACX;AACAC,OAAK,GAAE;AACH,QAAIC,KAAI,GAAIF,GAAG,CAAC,EAAD,CAAf;AAEAD,aAAS,CAAC,MAAM;AACZ;AACAD,WAAK,CAACK,GAAN,CAAU,iCAAV,EACCC,IADD,CACOC,MAAD,IAAY;AACdH,aAAK,CAACI,KAAN,GAAcD,MAAM,CAACE,IAArB;AACH,OAHD,EAGGC,KAHH,CAGUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAhB;AACH,OALD;AAMH,KARQ,CAAT;;AAUA,aAASC,OAAT,CAAiBC,EAAjB,EAAoBC,KAApB,EAA0B;AACtBjB,WAAK,CAACkB,MAAN,CACK,mCAAkCF,EAAG,EAD1C,EAGCV,IAHD,CAGM,MAAM;AACRF,aAAK,CAACI,KAAN,CAAYC,IAAZ,CAAiBU,MAAjB,CAAwBF,KAAxB,EAA8B,CAA9B;AACH,OALD,EAKGP,KALH,CAKUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OAPD;AAQJ;;AAEA,WAAO;AACHL,WADG;AACGW;AADH,KAAP;AAGJ;;AA7BW,CAAf","names":["axios","onMounted","ref","setup","mapel","get","then","result","value","data","catch","err","console","log","response","destroy","id","index","delete","splice"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Mapel\\IndexView.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n                <div class=\"main\">                    \r\n                    <div class=\"page-header\">\r\n                        <h4 class=\"page-title\">Data Mata