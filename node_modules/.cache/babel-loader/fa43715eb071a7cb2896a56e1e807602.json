{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  // \n  setup() {\n    //data binding\n    const ujian = reactive({\n      nama_ujian: '',\n      mapel_id: '',\n      kelas_id: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/ujian/${route.params.id}`).then(result => {\n        ujian.nama_ujian = result.data.data.nama_ujian;\n        ujian.mapel_id = result.data.data.mapel_id;\n        ujian.kelas_id = result.data.data.kelas_id;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let soal = ref([]);\n    onMounted(() => {\n      //get soal from api\n      axios.get('http://localhost:8000/api/soal').then(result => {\n        soal.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`http://localhost:8000/api/soalsoal/${id}`).then(() => {\n        soal.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      soal,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA0GA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACX;AACAC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIC,QAAQ,CAAC;AACnBC,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,cAAQ,EAAE;AAHS,KAAD,CAAtB;AAMA,UAAMC,UAAS,GAAIP,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMQ,MAAK,GAAIC,SAAS,EAAxB;AACA,UAAMC,KAAI,GAAIC,QAAQ,EAAtB;AAEAZ,aAAS,CAAC,MAAM;AACZD,WAAK,CAACc,GAAN,CAAW,mCAAkCF,KAAK,CAACG,MAAN,CAAaC,EAAG,EAA7D,EACCC,IADD,CACOC,MAAD,IAAY;AACdd,aAAK,CAACE,UAAN,GAAmBY,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBb,UAApC;AACAF,aAAK,CAACG,QAAN,GAAiBW,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBZ,QAAlC;AACAH,aAAK,CAACI,QAAN,GAAiBU,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBX,QAAlC;AACH,OALD,EAKGY,KALH,CAKUC,GAAD,IAAS;AACdC,eAAO,CAACC