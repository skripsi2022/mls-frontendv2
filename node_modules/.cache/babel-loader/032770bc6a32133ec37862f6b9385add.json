{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  // \n  setup() {\n    let nilaisiswa = ref([]);\n    onMounted(() => {\n      //get data-nilaisiswa from api\n      axios.get('/api/getNilaiGuru').then(result => {\n        nilaisiswa.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`/api/nilai/${id}`).then(() => {\n        nilaisiswa.value.data.splice(index, 1);\n        window.location.reload();\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      nilaisiswa,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACX;AACAC,OAAK,GAAE;AACC,QAAIC,UAAS,GAAIF,GAAG,CAAC,EAAD,CAApB;AAEAD,aAAS,CAAC,MAAM;AACZ;AACAD,WAAK,CAACK,GAAN,CAAU,mBAAV,EACCC,IADD,CACOC,MAAD,IAAY;AACdH,kBAAU,CAACI,KAAX,GAAmBD,MAAM,CAACE,IAA1B;AACH,OAHD,EAGGC,KAHH,CAGUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAhB;AACH,OALD;AAMH,KARQ,CAAT;;AAUH,aAASC,OAAT,CAAiBC,EAAjB,EAAoBC,KAApB,EAA0B;AACvBjB,WAAK,CAACkB,MAAN,CACK,cAAaF,EAAG,EADrB,EAGCV,IAHD,CAGM,MAAM;AACRF,kBAAU,CAACI,KAAX,CAAiBC,IAAjB,CAAsBU,MAAtB,CAA6BF,KAA7B,EAAmC,CAAnC;AACAG,cAAM,CAACC,QAAP,CAAgBC,MAAhB;AACH,OAND,EAMGZ,KANH,CAMUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OARD;AASJ;;AAEA,WAAO;AACHL,gBADG;AACQW;AADR,KAAP;AAGJ;;AA9BW,CAAf","names":["axios","onMounted","ref","setup","nilaisiswa","get","then","result","value","data","catch","err","console","log","response","destroy","id","index","delete","splice","window","location","reload"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Guru\\Nilai\\IndexView.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n        <div class=\"main\">\r\n            <div class=\"page-header\"