r\n            </div>\r\n            <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                    <h4>Daftar Nilai</h4>\r\n                    <div class=\"mt-4\">\r\n                        <table id=\"data-table\" class=\"table data-table\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>No.</th>\r\n                                    <th>Nama</th>\r\n                                    <th>Mata Pelajaran</th>\r\n                                    <th>Nilai</th>\r\n                                    <th>Action</th>\r\n                                </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                                <tr v-for=\"(nilaisiswa, index) in nilaisiswa.data\" :key=\"index\">\r\n                                    <td>{{++index}}</td>\r\n                                    <td>{{nilaisiswa.siswa.nama_siswa}}</td>\r\n                                    <td>{{nilaisiswa.ujian.nama_ujian}}</td>\r\n                                    <td>{{nilaisiswa.nilai}}</td>\r\n                                    <td>\r\n                                        <button class=\"btn btn-danger btn-sm\"\r\n                                            @click.prevent=\"destroy(nilaisiswa.id_nilai, index)\">Delete</button>\r\n                                    </td>\r\n                                </tr>\r\n                            </tbody>\r\n                        </table>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <!-- Footer START -->\r\n        <div class=\"footer fixed-bottom \">\r\n            <div class=\"footer-content justify-content-md-end\">\r\n                <p class=\"mb-0\">Copyright Â© 2022. All rights reserved.</p>\r\n            </div>\r\n        </div>\r\n        <!-- Footer End -->\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\nimport {onMounted, ref} from 'vue'\r\n\r\nexport default {\r\n    // \r\n    setup(){\r\n            let nilaisiswa = ref([]);\r\n\r\n            onMounted(() => {\r\n                //get data-nilaisiswa from api\r\n                axios.get('/api/getNilaiGuru')\r\n                .then((result) => {\r\n                    nilaisiswa.value = result.data\r\n                }).catch((err) => {\r\n                    console.log(err.response)\r\n                });\r\n            });\r\n\r\n         function destroy(id,index){\r\n            axios.delete(\r\n                `/api/nilai/${id}`\r\n            )\r\n            .then(() => {\r\n                nilaisiswa.value.data.splice(index,1)\r\n                window.location.reload();\r\n            }).catch((err) => {\r\n                console.log(err.response.data);\r\n            });\r\n        } \r\n\r\n        return {\r\n            nilaisiswa,destroy\r\n        }\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}