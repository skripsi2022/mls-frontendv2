{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  // \n  data() {\n    return {\n      //state loggedIn with localStorage\n      loggedIn: localStorage.getItem('loggedIn'),\n      //state token\n      token: localStorage.getItem('token'),\n      //state user logged In\n      user: localStorage.getItem('user')\n    };\n  },\n\n  created() {\n    axios.get('http://localhost:8000/api/user', {\n      headers: {\n        'Authorization': 'Bearer ' + this.token\n      }\n    }).then(response => {\n      console.log(response);\n      this.user = response.data.id; // assign response to state user\n    });\n  },\n\n  setup() {\n    let mapel = ref([]);\n    onMounted(() => {\n      //get mapel by guru\n      axios.get('http://localhost:8000/api/mapelGuru', {\n        params: {\n          id: this.user\n        }\n      }).then(result => {\n        mapel.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`http://localhost:8000/api/mapel/${id}`).then(() => {\n        mapel.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      mapel,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA8FA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACX;AACCC,MAAI,GAAG;AACJ,WAAO;AACH;AACAC,cAAQ,EAAEC,YAAY,CAACC,OAAb,CAAqB,UAArB,CAFP;AAGH;AACAC,WAAK,EAAEF,YAAY,CAACC,OAAb,CAAqB,OAArB,CAJJ;AAKH;AACAE,UAAI,EAAEH,YAAY,CAACC,OAAb,CAAqB,MAArB;AANH,KAAP;AAQH,GAXU;;AAaXG,SAAO,GAAG;AACNT,SAAK,CAACU,GAAN,CAAU,gCAAV,EAA4C;AAACC,aAAO,EAAE;AAAC,yBAAiB,YAAU,KAAKJ;AAAjC;AAAV,KAA5C,EACCK,IADD,CACMC,QAAO,IAAK;AACdC,aAAO,CAACC,GAAR,CAAYF,QAAZ;AACA,WAAKL,IAAL,GAAYK,QAAQ,CAACV,IAAT,CAAca,EAA1B,CAFc,CAEe;AAChC,KAJD;AAKH,GAnBU;;AAoBXC,OAAK,GAAE;AACH,QAAIC,KAAI,GAAIhB,GAAG,CAAC,EAAD,CAAf;AAEAD,aAAS,CAAC,MAAM;AACZ;AACAD,WAAK,CAACU,