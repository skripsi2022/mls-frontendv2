});\r\n        });\r\n\r\n        function destroy(id,index){\r\n            axios.delete(\r\n                `http://localhost:8000/api/mapel/${id}`\r\n            )\r\n            .then(() => {\r\n                mapel.value.data.splice(index,1)\r\n            }).catch((err) => {\r\n                console.log(err.response.data);\r\n            });\r\n        } \r\n\r\n        return {\r\n            mapel,destroy,\r\n        }\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}