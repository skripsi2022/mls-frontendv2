div>\r\n                                <div class=\"breadcrumb-item\">Detail</div>\r\n                                </div>                         \r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-lg-4\">\r\n                             <div class=\"d-grid d-md-flex justify-content-md-end\">\r\n                                <router-link :to=\"{name: 'ujian.index'}\" class=\"btn btn-warning me-md-end\" type=\"button\"><i class=\"icon-arrow-left-circle feather\"></i>Kembali</router-link>\r\n                             </div>\r\n                        </div>\r\n                    </div>\r\n                    <br>\r\n                    <div class=\"card\">\r\n                        <div class=\"card-body\">\r\n                            <h4>Detail Ujian</h4>\r\n                            <br>\r\n                            <form class=\"row g-3\" @submit.prevent=\"update()\">\r\n                                <div class=\"col-md-6\">\r\n                                    <label for=\"\" class=\"form-label\">Nama Mapel</label>\r\n                                    <input type=\"text\" class=\"form-control\" v-model=\"mapel.nama_mapel\">\r\n                                    <div v-if=\"Validation.nama_mapel\" class=\"text-danger\">\r\n                                        {{Validation.nama_mapel[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                 <div class=\"col-md-4\">\r\n                                    <label for=\"\" class=\"form-label\">Guru</label>\r\n                                    <select name=\"guru\" class=\"form-select\" v-model=\"mapel.guru_id\">\r\n                                        <option v-for=\"(guru, index) in guru.data\" :key=\"index\" :value=\"guru.id_guru\">{{guru.nama_guru}}</option>\r\n                                    </select>\r\n                                    <div v-if=\"Validation.guru_id\" class=\"text-danger\">\r\n                                        {{Validation.guru_id[0]}}\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"col-12\">\r\n                                    <button class=\"btn btn-warning\" type=\"submit\">Update</button>\r\n                                </div>\r\n                            </form>  \r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer START -->\r\n                <div class=\"footer fixed-bottom \">\r\n                    <div class=\"footer-content justify-content-md-end\">\r\n                        <p class=\"mb-0\">Copyright Â© 2022. All rights reserved.</p>\r\n                    </div>\r\n                </div>\r\n                <!-- Footer End -->\r\n            </div>\r\n</template>\r\n\r\n<script>\r\nimport {reactive ,ref, onMounted} from 'vue'\r\nimport { useRoute, useRouter} from 'vue-router'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    setup(){\r\n        //data binding\r\n        const ujian = reactive({\r\n            nama_ujian: '',\r\n            mapel_id: '',\r\n            kelas_id: '',\r\n        });\r\n\r\n        const Validation = ref([]);\r\n\r\n        const router = useRouter();\r\n        const route = useRoute();\r\n\r\n        onMounted(() => {\r\n            axios.get(`http://localhost:8000/api/ujian/${route.params.id}`)\r\n            .then((result) => {\r\n                ujian.nama_ujian = result.data.data.nama_ujian\r\n                ujian.mapel_id = result.data.data.mapel_id\r\n                ujian.mapel_id = result.data.data.mapel_id\r\n            }).catch((err) => {\r\n                console.log(err.response.data)\r\n            });\r\n        });\r\n\r\n        let guru = ref([]);\r\n\r\n        onMounted(() => {\r\n            //get siswa from api\r\n            axios.get('http://localhost:8000/ujian/guru')\r\n            .then((result) => {\r\n                ujian.value = result.data\r\n            }).catch((err) => {\r\n                console.log(err.response)\r\n            });\r\n        });\r\n\r\n\r\n        function update() {\r\n            axios.put(\r\n                `http://localhost:8000/api/mapel/${route.params.id}`,\r\n                mapel\r\n            )\r\n             .then(() => {\r\n                router.push({\r\n                    name : 'mapel.index'\r\n                })\r\n            }).catch((err) => {\r\n                Validation.value = err.response.data\r\n            });\r\n        }\r\n\r\n        return {\r\n            mapel,\r\n            guru,\r\n            Validation,\r\n            router,\r\n            update\r\n        }\r\n    }    \r\n}\r\n</script>"]},"metadata":{},"sourceType":"module"}