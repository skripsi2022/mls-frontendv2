{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const ujian = reactive({\n      nama_ujian: '',\n      mapel_id: '',\n      kelas_id: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/ujian/${route.params.id}`).then(result => {\n        ujian.nama_ujian = result.data.data.nama_ujian;\n        ujian.mapel_id = result.data.data.mapel_id;\n        ujian.mapel_id = result.data.data.mapel_id;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let guru = ref([]);\n    onMounted(() => {\n      //get siswa from api\n      axios.get('http://localhost:8000/ujian/guru').then(result => {\n        ujian.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function update() {\n      axios.put(`http://localhost:8000/api/mapel/${route.params.id}`, mapel).then(() => {\n        router.push({\n          name: 'mapel.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      mapel,\n      guru,\n      Validation,\n      router,\n      update\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6DA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIP,QAAQ,CAAC;AACnBQ,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,cAAQ,EAAE;AAHS,KAAD,CAAtB;AAMA,UAAMC,UAAS,GAAIV,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMW,MAAK,GAAIR,SAAS,EAAxB;AACA,UAAMS,KAAI,GAAIV,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACS,GAAN,CAAW,mCAAkCD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA7D,EACCC,IADD,CACOC,MAAD,IAAY;AACdX,aAAK,CAACC,UAAN,GAAmBU,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBX,UAApC;AAC