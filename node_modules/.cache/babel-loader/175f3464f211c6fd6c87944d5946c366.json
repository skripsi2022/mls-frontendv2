{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  // \n  setup() {\n    let jurusan = ref([]);\n    onMounted(() => {\n      //get jurusan from api\n      axios.get('http://localhost:8000/api/jurusan').then(result => {\n        jurusan.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`http://localhost:8000/api/jurusan/${id}`).then(() => {\n        jurusan.value.data.splice(index, 1);\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      jurusan,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACX;AACAC,OAAK,GAAE;AACH,QAAIC,OAAM,GAAIF,GAAG,CAAC,EAAD,CAAjB;AAEAD,aAAS,CAAC,MAAM;AACZ;AACAD,WAAK,CAACK,GAAN,CAAU,mCAAV,EACCC,IADD,CACOC,MAAD,IAAY;AACdH,eAAO,CAACI,KAAR,GAAgBD,MAAM,CAACE,IAAvB;AACH,OAHD,EAGGC,KAHH,CAGUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAhB;AACH,OALD;AAMH,KARQ,CAAT;;AAUA,aAASC,OAAT,CAAiBC,EAAjB,EAAoBC,KAApB,EAA0B;AACtBjB,WAAK,CAACkB,MAAN,CACK,qCAAoCF,EAAG,EAD5C,EAGCV,IAHD,CAGM,MAAM;AACRF,eAAO,CAACI,KAAR,CAAcC,IAAd,CAAmBU,MAAnB,CAA0BF,KAA1B,EAAgC,CAAhC;AACH,OALD,EAKGP,KALH,CAKUC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaL,IAAzB;AACH,OAPD;AAQJ;;AAEA,WAAO;AACHL,aADG;AACKW;AADL,KAAP;AAGJ;;AA7BW,CAAf","names":["axios","onMounted","ref","setup","jurusan","get","then","result","value","data","catch","err","console","log","response","destroy","id","index","delete","splice"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Jurusan\\IndexView.vue"],"sourcesContent":["<template>\r\n    <div class=\"content\">\r\n                <div class=\"main\">                    \r\n                    <div class=\"page-header\">\r\n                        <h4 class=\"pa