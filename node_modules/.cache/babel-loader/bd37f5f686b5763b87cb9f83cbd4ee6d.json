{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    //data binding\n    const guru = reactive({\n      nama_guru: '',\n      notelp_guru: '',\n      email_guru: '',\n      alamat_guru: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    onMounted(() => {\n      axios.get(`http://localhost:8000/api/guru/${route.params.id}`).then(result => {\n        guru.nama_siswa = result.data.data.nama_siswa, guru.nis_siswa = result.data.data.nis_siswa, guru.kelas_id = result.data.data.kelas_id, guru.alamat_siswa = result.data.data.alamat_siswa;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n    let kelas = ref([]);\n    onMounted(() => {\n      //get siswa from api\n      axios.get('http://localhost:8000/api/kelas').then(result => {\n        kelas.value = result.data;\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    });\n\n    function update() {\n      axios.put(`http://localhost:8000/api/siswa/${route.params.id}`, guru).then(() => {\n        router.push({\n          name: 'guru.index'\n        });\n      }).catch(err => {\n        Validation.value = err.response.data;\n      });\n    }\n\n    return {\n      guru,\n      Validation,\n      router,\n      update\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyEA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,IAAG,GAAIP,QAAQ,CAAC;AAClBQ,eAAS,EAAE,EADO;AAElBC,iBAAW,EAAE,EAFK;AAGlBC,gBAAU,EAAE,EAHM;AAIlBC,iBAAW,EAAE;AAJK,KAAD,CAArB;AAOA,UAAMC,UAAS,GAAIX,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMY,MAAK,GAAIT,SAAS,EAAxB;AACA,UAAMU,KAAI,GAAIX,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACU,GAAN,CAAW,kCAAiCD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAA5D,EACCC,IADD,CACOC,MAAD,IAAY;A