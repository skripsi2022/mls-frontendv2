{"ast":null,"code":"import { reactive, ref, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport axios from 'axios';\nimport Swal from 'sweetalert2';\nexport default {\n  setup() {\n    //data binding\n    const siswa = reactive({\n      nama_siswa: '',\n      nis_siswa: '',\n      kelas_id: '',\n      alamat_siswa: ''\n    });\n    const Validation = ref([]);\n    const router = useRouter();\n\n    function store() {\n      axios.post('/api/siswa', siswa).then(() => {\n        Swal.fire({\n          title: 'Berhasil!',\n          text: 'Siswa Berhasil ditambahkan !',\n          icon: 'success',\n          confirmButtonText: 'Lanjut !'\n        });\n        router.push({\n          name: 'siswa.index'\n        });\n      }).catch(err => {\n        Swal.fire({\n          title: 'Berhasil!',\n          text: 'Siswa Berhasil ditambahkan !',\n          icon: 'success',\n          confirmButtonText: 'Lanjut !'\n        });\n        Validation.value = err.response.data;\n      });\n    }\n\n    let kelas = ref([]);\n    onMounted(() => {\n      //get siswa from api\n      axios.get('/api/kelas').then(result => {\n        kelas.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n    return {\n      siswa,\n      kelas,\n      Validation,\n      router,\n      store\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+EA,SAAQA,QAAR,EAAkBC,GAAlB,EAAsBC,SAAtB,QAAsC,KAAtC;AACA,SAASC,SAAT,QAAyB,YAAzB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,IAAP,MAAiB,aAAjB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH;AACA,UAAMC,KAAI,GAAIP,QAAQ,CAAC;AACnBQ,gBAAU,EAAE,EADO;AAEnBC,eAAS,EAAE,EAFQ;AAGnBC,cAAQ,EAAE,EAHS;AAInBC,kBAAY,EAAE;AAJK,KAAD,CAAtB;AAOA,UAAMC,UAAS,GAAIX,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMY,MAAK,GAAIV,SAAS,EAAxB;;AAEA,aAASW,KAAT,GAAiB;AACbV,WAAK,CAACW,IAAN,CACI,YADJ,EAEIR,KAFJ,EAIES,IAJF,CAIO,MAAM;AACRX,YAAI,CAACY,IAAL,CAAU;AACNC,eAAK,EAAE,WADD;AAENC,cAAI,EAAE,8BAFA;AAGNC,cAAI,EAAE,SAHA;AAINC,2BAAiB,EAAE;AAJb,SAAV;AAODR,cAAM,CAA