{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  // \n  setup() {\n    let ujian = ref([]);\n    onMounted(() => {\n      //get ujian from api\n      console.log(\"ujian id : \" + localStorage.ujian_id);\n      axios.get('/api/ujian').then(result => {\n        ujian.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    function destroy(id, index) {\n      axios.delete(`/api/ujian/${id}`).then(() => {\n        ujian.value.data.splice(index, 1);\n        window.location.reload();\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      ujian,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AACX;AACAC,OAAK,GAAE;AACH,QAAIC,KAAI,GAAIF,GAAG,CAAC,EAAD,CAAf;AAEAD,aAAS,CAAC,MAAM;AACZ;AACGI,aAAO,CAACC,GAAR,CAAY,gBAAgBC,YAAY,CAACC,QAAzC;AACHR,WAAK,CAACS,GAAN,CAAU,YAAV,EACCC,IADD,CACOC,MAAD,IAAY;AACdP,aAAK,CAACQ,KAAN,GAAcD,MAAM,CAACE,IAArB;AAEH,OAJD,EAIGC,KAJH,CAIUC,GAAD,IAAS;AACdV,eAAO,CAACC,GAAR,CAAYS,GAAG,CAACC,QAAhB;AACH,OAND;AAOH,KAVQ,CAAT;;AAYA,aAASC,OAAT,CAAiBC,EAAjB,EAAoBC,KAApB,EAA0B;AACtBnB,WAAK,CAACoB,MAAN,CACK,cAAaF,EAAG,EADrB,EAGCR,IAHD,CAGM,MAAM;AACRN,aAAK,CAACQ,KAAN,CAAYC,IAAZ,CAAiBQ,MAAjB,CAAwBF,KAAxB,EAA8B,CAA9B;AACAG,cAAM,CAACC,QAAP,CAAgBC,MAAhB;AACH,OAND,EAMGV,KANH,CAMUC,GAAD,IAAS;AACdV,eAAO,CAACC,GAAR,CAAYS,GAAG,CAACC,QAAJ,CAAaH,IAAzB;AACH,OARD;AASJ;;AAEA,WAAO;AACHT,WADG;AACGa;AADH,KAAP;AAGJ;;AAhCW,CAAf","names":["axios","onMounted","ref","setup","ujian","console","log","localStorage","ujian_id","get","then","result","value","data","catch","err","response","destroy","id","index","delete","splice","window","location","reload"],"sourceRoot":"","sources":["E:\\KULIAH\\SEKERIPSI\\PROGRESS\\OTW\\FRONTEND\\mls-frontend_v2\\src\\views\\Dashboard\\Ujian\\IndexView.vue"],"sourcesContent":["<template>\r\n    <di