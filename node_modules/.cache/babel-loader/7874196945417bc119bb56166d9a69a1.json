axios.post(\n      //     '/api/soal',\n      //     addsoal\n      // )\n      //  .then(() => {\n      //     localStorage.removeItem('ujian_id');\n      //     router.push({\n      //         name : 'ujian.index'\n      //     })\n      // }).catch((err) => {\n      //     Validation.value = err.response.data\n      // });\n    }\n\n    function destroy(id, index) {\n      axios.delete(`/api/soal/${id}`).then(() => {\n        soal.value.data.splice(index, 1);\n        router.push({\n          name: 'ujian.index'\n        });\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      ujian,\n      soal,\n      back,\n      store,\n      destroy,\n      addsoal,\n      Validation,\n      router\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiLA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,QAAuC,KAAvC;AACA,SAASC,QAAT,EAAmBC,SAAnB,QAAmC,YAAnC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,SAAM,EAAI;AACNC,YAAQ,GAAE;AACNC,aAAO,CAACC,GAAR,CAAY,QAAZ,EADM,CAEN;;AACAC,YAAM,CAACC,OAAP,CAAeC,IAAf;AACJ;;AALM,GADC;;AASXC,OAAK,GAAE;AAGH;AACA,UAAMC,KAAI,GAAId,QAAQ,CAAC;AACnBe,gBAAU,EAAE,EADO;AAEnBC,cAAQ,EAAE,EAFS;AAGnBC,WAAK,EAAE,EAHY;AAInBC,WAAK,EAAE;AAJY,KAAD,CAAtB;AAQA,UAAMC,UAAS,GAAIlB,GAAG,CAAC,EAAD,CAAtB;AAEA,UAAMmB,MAAK,GAAIhB,SAAS,EAAxB;AACA,UAAMiB,KAAI,GAAIlB,QAAQ,EAAtB;AAEAD,aAAS,CAAC,MAAM;AACZG,WAAK,CAACiB,GAAN,CAAW,cAAaD,KAAK,CAACE,MAAN,CAAaC,EAAG,EAAxC,EACCC,IADD,CACOC,MAAD,IAAY;AACd;AAEAZ,aAAK,CAACE,QAAN,GAAiBU,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBX,QAAlC;AACAF,aAAK,CAACC,UAAN,GAAmBW,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBZ,UAApC;AACAD,aAAK,CAACG,KAAN,GAAcS,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBV,KAAjB,CAAuBW,UAArC;AACAd,aAAK,CAACI,KAAN,GAAcQ,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBT,KAAjB,CAAuBW,UAArC,CANc,CAQd;;AACAC,oBAAY,CAACC,OAAb,CAAqB,UAArB,EAAiCjB,KAAK,CAACE,QAAvC;AAEAR,eAAO,CAACC,GAAR,CAAY,sBAAsBK,KAAK,CAACE,QAAxC;AACAR,eAAO,CAACC,GAAR,CAAY,yBAAyBiB,MAAM,CAACC,IAAP,CAAYA,IAAZ,CAAiBX,QAAtD;AAEH,OAfD,EAeGgB,KAfH,CAeUC,GAAD,IAAS;AACdzB,eAAO,C