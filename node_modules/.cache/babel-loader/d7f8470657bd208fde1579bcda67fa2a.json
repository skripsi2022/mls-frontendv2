{"ast":null,"code":"import axios from 'axios';\nimport { onMounted, ref } from 'vue';\nexport default {\n  methods: {\n    destroy1(id) {\n      axios.delete(`/api/ujian/${id}`).then(() => {\n        //  ujian.value.data.splice(index, 1)           \n        this.$swal({\n          title: 'Success!',\n          text: 'Data Ujian Berhasil dihapus !',\n          icon: 'success',\n          confirmButtonText: 'Lanjut !'\n        });\n      }).catch(err => {\n        console.log(err.response.data);\n        this.$swal({\n          title: 'error!',\n          text: 'Kosongkan Soal terlebih dahulu !',\n          icon: 'error',\n          confirmButtonText: 'Lanjut !'\n        });\n      });\n    }\n\n  },\n\n  setup() {\n    let ujian = ref([]);\n    onMounted(() => {\n      //get ujian from api\n      console.log(\"ujian id : \" + localStorage.ujian_id);\n      axios.get('/api/ujian').then(result => {\n        ujian.value = result.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    });\n\n    async function destroy(id, index) {\n      axios.delete(`/api/ujian/${id}`).then(() => {\n        ujian.value.data.splice(index, 1);\n        window.location.reload();\n      }).catch(err => {\n        console.log(err.response.data);\n      });\n    }\n\n    return {\n      ujian,\n      destroy\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsGA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,QAA6B,KAA7B;AAEA,eAAe;AAEVC,SAAM,EAAI;AACPC,YAAQ,CAACC,EAAD,EAAI;AAEPL,WAAK,CAACM,MAAN,CACK,cAAaD,EAAG,EADrB,EAGKE,IAHL,CAGU,MAAM;AACT;AACC,aAAKC,KAAL,CAAW;AACRC,eAAK,EAAE,UADC;AAERC,cAAI,EAAE,+BAFE;AAGRC,cAAI,EAAE,SAHE;AAIRC,2BAAiB,EAAE;AAJX,SAAX;AAMH,OAXL,EAWOC,KAXP,CAWcC,GAAD,IAAS;AACdC,eAAO,CAACC,GAAR,CAAYF,GAAG,CAACG,QAAJ,CAAaC,IAAzB;AACA,aAAKV,KAAL,CAAW;AACRC,eAAK,EAAE,QADC;AAERC,cAAI,EAAE,kCAFE;AAGRC,cAAI,EAAE,OAHE;AAIRC,2BAAiB,EAAE;AAJX,SAAX;AAMH,OAnBL;AAoBJ;;AAvBM,GAFA;;AA2BXO,OAAK,GAAE;AACH,QAAIC,KAAI,GAAIlB,GAAG,CAAC,EAAD,CAAf;AAEAD,aAAS,CAAC,M